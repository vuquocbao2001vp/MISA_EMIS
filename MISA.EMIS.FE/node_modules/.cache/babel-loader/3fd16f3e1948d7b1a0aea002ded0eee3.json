{"ast":null,"code":"import TablePaging from \"../base/TablePaging.vue\";\nimport BaseButton from \"../base/BaseButton.vue\";\nimport BaseInput from \"../base/BaseInput.vue\";\nimport StaffDialog from \"../dialog/StaffDialog.vue\";\nimport BaseCheckbox from \"../base/BaseCheckbox.vue\";\nimport BaseLoader from \"../base/BaseLoader.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    BaseLoader,\n    TablePaging,\n    BaseButton,\n    BaseInput,\n    StaffDialog,\n    BaseCheckbox\n  },\n\n  created() {\n    this.loadData();\n  },\n\n  data() {\n    return {\n      isHideLoader: false,\n      isShowDialog: false,\n      staffs: {},\n      staffSelected: {},\n      staffSelectedId: null,\n      btnSaveMode: 0,\n      tot\n    };\n  },\n\n  methods: {\n    /**\r\n     * thực hiện tải dữ liệu thông tin giáo viên và hiển thị trên bản\r\n     * Author: VQBao - 21/7/2022\r\n     */\n    loadData() {\n      this.isHideLoader = false;\n\n      try {\n        var me = this; // lấy dữ liệu từ api\n\n        axios.get(\"https://cukcuk.manhnv.net/api/v1/Employees\").then(function (respond) {\n          // ẩn loading\n          me.isHideLoader = true;\n          me.staffs = respond.data;\n        }).catch(function (res) {\n          console.log(res);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * khi bấm thêm thì form chi tiết giáo viên hiện ra\r\n     * Author: VQBao - 21/7/2022\r\n     */\n    btnAddOnClick() {\n      // btnSaveMode = 1 => thực hiện thêm mới\n      this.btnSaveMode = 1;\n      this.showStaffDialog(true);\n      this.staffSelected = {};\n      this.staffSelectedId = null;\n      var me = this; // gọi api lấy mã nhân viên mới\n\n      axios.get(\"https://cukcuk.manhnv.net/api/v1/Employees/NewEmployeeCode\").then(response => {\n        me.staffSelected.EmployeeCode = response.data;\n      }).catch(() => {\n        console.error(\"Lấy mã nhân viên mới không thành công\");\n      });\n    },\n\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của form thêm mới\r\n     * @param {Boolean} iShow - true: hiển thị, false: ẩn đi\r\n     * Author: VQBao - 21/7/2022\r\n     */\n    showStaffDialog(isShow) {\n      this.isShowDialog = isShow;\n    },\n\n    /**\r\n     * Khi nhấn vào tên hoặc icon sửa trong bảng, hiện form thông tin chi tiết\r\n     * Author: VQBao - 21/7/2022\r\n     */\n    rowOnClick(staff) {\n      // btnSaveMode = 0 => thực hiện sửa\n      this.btnSaveMode = 0;\n      this.staffSelected = staff;\n      this.staffSelectedId = staff.EmployeeId;\n      this.showStaffDialog(true);\n    },\n\n    /**\r\n     * khi bấm vào icon xóa thì thực hiện xóa\r\n     * Author: VQBao - 21/7/2022\r\n     */\n    removeOnClick(staff) {\n      var me = this;\n      me.isHideLoader = false;\n      me.staffSelected = staff;\n      me.staffSelectedId = staff.EmployeeId;\n      axios.delete(`https://cukcuk.manhnv.net/api/v1/Employees/${me.staffSelectedId}`).then(() => {\n        me.loadData();\n        console.log(\"Xóa xong\");\n      }).catch(() => {\n        console.log(\"Xóa lỗi\");\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2FA,OAAOA,WAAP,MAAwB,yBAAxB;AACA,OAAOC,UAAP,MAAuB,wBAAvB;AACA,OAAOC,SAAP,MAAsB,uBAAtB;AACA,OAAOC,WAAP,MAAwB,2BAAxB;AACA,OAAOC,YAAP,MAAyB,0BAAzB;AACA,OAAOC,UAAP,MAAuB,wBAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,UAAU,EAAE;IACVF,UADU;IAEVL,WAFU;IAGVC,UAHU;IAIVC,SAJU;IAKVC,WALU;IAMVC;EANU,CADC;;EASbI,OAAO,GAAG;IACR,KAAKC,QAAL;EACD,CAXY;;EAYbC,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,KADT;MAELC,YAAY,EAAE,KAFT;MAGLC,MAAM,EAAE,EAHH;MAILC,aAAa,EAAE,EAJV;MAKLC,eAAe,EAAE,IALZ;MAMLC,WAAW,EAAE,CANR;MAOLC;IAPK,CAAP;EASD,CAtBY;;EAuBbC,OAAO,EAAE;IACP;;;;IAIAT,QAAQ,GAAE;MACR,KAAKE,YAAL,GAAoB,KAApB;;MACA,IAAI;QACJ,IAAIQ,EAAC,GAAI,IAAT,CADI,CAEJ;;QACAb,KAAI,CACDc,GADH,CACO,4CADP,EAEGC,IAFH,CAEQ,UAAUC,OAAV,EAAmB;UACvB;UACAH,EAAE,CAACR,YAAH,GAAkB,IAAlB;UACAQ,EAAE,CAACN,MAAH,GAAYS,OAAO,CAACZ,IAApB;QACD,CANH,EAOGa,KAPH,CAOS,UAAUC,GAAV,E