ck","onKeyup","$options","_toDisplayString","selectedItem","onClick","$data","_createVNode","_component_BaseCheckbox","_hoisted_3","_Fragment","_renderList","item","index","key","propKey","propValue"],"sourceRoot":"","sources":["E:\\MisaProject\\W2022_06\\misa_emis\\src\\components\\base\\BaseTagboxCustom.vue"],"sourcesContent":["<template>\n  <div class=\"tagbox\" @keyup.enter=\"showListData\">\n    <div class=\"tagbox-content\">{{this.selectedItem}}</div>\n    <div\n      class=\"tagbox-button icon-center icon-Chevron\"\n      @click=\"showListData\"\n    ></div>\n    <div class=\"tagbox-data\" :class=\"{ 'is-show': isShowListData }\">\n      <div class=\"tagbox-item\">\n        <BaseCheckbox :class=\"isCheckAll ? 'checkbox-active' : 'checkbox-inactive'\" @click=\"selectAllItem\"/>\n        <div class=\"tagbox-item-content\">Tất cả</div>\n      </div>\n      <div class=\"tagbox-item\" v-for=\"(item, index) in data\" :key=\"item[this.propKey]\" @click=\"selectDataItem(item, index)\">\n        <BaseCheckbox :class=\"isCheck[index] ? 'checkbox-active' : 'checkbox-inactive'\" @click=\"selectDataItem(item, index)\"/>\n        <div class=\"tagbox-item-content\">{{item[this.propValue]}}</div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  props: {\n    url: String,\n    propKey: String,\n    propValue: String,\n  },\n  data() {\n    return {\n      data: [],\n      isShowListData: false,\n      selectedItem: null,\n      selectedItemList: [],\n      isCheck: [false],\n      isCheckAll: false,\n    };\n  },\n  created() {\n    var me = this;\n    /**\n     * Thực hiện lấy dữ liệu từ api\n     * Author: VQBao - 10/8/2022\n     */\n    axios\n      .get(me.url)\n      .then((res) => {\n        me.data = res.data;\n        me.selectedItem = res.data[1][me.propValue];\n      })\n      .catch((err) => {\n        console.error(err);\n      });\n  },\n  methods: {\n    /**\n     * Thực hiện ẩn hiện danh sách các select\n     * Author: VQBao - 10/8/2022\n     */\n    showListData() {\n      this.isShowListData = !this.isShowListData;\n    },\n    /**\n     * Thực hiện chọn item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectDataItem(item, index) {\n      this.selectedItem = item[this.propValue];\n      if(!this.isCheck[index]) {\n        this.isCheck[index] = true;\n        this.selectedItemList.push(item[this.propValue]);\n        if(this.selectedItemList.length == this.data.length){\n          this.isCheckAll = true;\n        }\n        console.log(this.selectedItemList);\n      } else {\n        this.isCheck[index] = false;\n        let id = this.selectedItemList.indexOf(this.selectedItem);\n        this.selectedItemList.splice(id, 1);\n        this.isCheckAll = false;\n        console.log(this.selectedItemList);\n      }\n    },\n    /**\n     * Thực hiện chọn tất cả option trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectAllItem() {\n      if(!this.isCheckAll) {\n        this.isCheckAll = true;\n        this.selectedItemList = [];\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = true;\n          let item = this.data[i];\n          this.selectedItemList.push(item[this.propValue]);\n        }\n        console.log(this.selectedItemList);\n      } else {\n        this.isCheckAll = false;\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = false;\n        }\n        this.selectedItemList = [];\n        console.log(this.selectedItemList);\n      }\n      console.log(\"ok\");\n    }\n  },\n};\n</script>\n<style scoped>\n.is-show {\n  display: block !important;\n}\n.tagbox {\n  position: relative;\n  width: 200px;\n  box-sizing: border-box;\n  display: flex;\n  flex-direction: column;\n}\n.tagbox-content {\n  width: 200px;\n  height: 32px;\n  border: 1px solid;\n  border-radius: 4px;\n  border-color: var(--button-border);\n  box-sizing: border-box;\n  display: flex;\n  align-items: center;\n  padding: 0 16px;\n}\n\n.tagbox-button {\n  width: 32px;\n  height: 32px;\n  position: absolute;\n  right: 0px;\n  top: 0px;\n  cursor: pointer;\n}\n\n.tagbox-button:hover {\n  background-color: var(--background);\n}\n\n.tagbox-data {\n  display: none;\n  width: 200px;\n  max-height: 200px;\n  box-sizing: border-box;\n  box-shadow: 0.5px 0.5px 1px 1px rgb(222, 222, 222);\n  border-radius: 4px;\n  padding-top: 4px;\n  padding-bottom: 4px;\n  position: absolute;\n  top: 36px;\n  overflow-y: scroll;\n  overflow-x: hidden;\n  background-color: var(--white-background);\n  z-index: 1;\n  outline: none;\n}\n\n.tagbox-item {\n  width: 200px;\n  height: 32px;\n  box-sizing: border-box;\n  cursor: pointer;\n  padding: 0 16px;\n  display: flex;\n  align-items: center;\n}\n\n.tagbox-item-content {\n  margin-left: 12px;\n}\n\n.tagbox-item:hover {\n  background-color: var(--grid-line-focus-hover);\n}\n\n/* custom scroll*/ \n/* width */\n.tagbox-data::-webkit-scrollbar {\n  width: 4px;\n  height: 4px;\n}\n\n/* Track */\n.tagbox-data::-webkit-scrollbar-track {\n  border-radius: 4px;\n}\n\n/* Handle */\n.tagbox-data::-webkit-scrollbar-thumb {\n  background: var(--scroll-color);\n  border-radius: 4px;\n}\n/* hover */\n.tagbox-data::-webkit-scrollbar-thumb:hover {\n  background: var(--scroll-hover-color);\n}\n</style>"]},"metadata":{},"sourceType":"module"}