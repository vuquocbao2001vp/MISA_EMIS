lectedItemList.indexOf(this.selectedItem);\n        this.selectedItemList.splice(id, 1);\n        this.isCheckAll = false;\n      }\n    },\n    /**\n     * Thực hiện chọn tất cả item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectAllItem() {\n      if (!this.isCheckAll) {\n        this.isCheckAll = true;\n        this.selectedItemList = [];\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = true;\n          let item = this.data[i];\n          this.selectedItemList.push(item[this.propValue]);\n        }\n      } else {\n        this.isCheckAll = false;\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = false;\n        }\n        this.selectedItemList = [];\n      }\n    },\n  },\n};\n</script>\n<style scoped>\n.selected-item-tag {\n  position: relative;\n  min-width: fit-content;\n  height: 24px;\n  background-color: var(--button-grey-background);\n  display: flex;\n  align-items: center;\n  padding-left: 8px;\n  border-radius: 4px;\n  margin-right: 8px;\n  box-sizing: border-box;\n}\n\n.selected-item-tag-icon {\n  height: 24px;\n  width: 24px;\n  cursor: pointer;\n}\n\n.is-show {\n  display: block !important;\n}\n.tagbox {\n  position: relative;\n  width: 200px;\n  box-sizing: border-box;\n  display: flex;\n  flex-direction: column;\n}\n.tagbox-content {\n  width: 200px;\n  height: 32px;\n  border: 1px solid;\n  border-radius: 4px;\n  border-color: var(--button-border);\n  box-sizing: border-box;\n  padding: 0 4px;\n}\n\n.tagbox-content-text {\n  height: 32px;\n  width: 168px;\n  display: flex;\n  align-items: center;\n  box-sizing: border-box;\n  overflow-x: scroll;\n  overflow-y: hidden;\n}\n\n.tagbox-button {\n  width: 32px;\n  height: 32px;\n  position: absolute;\n  right: 0px;\n  top: 0px;\n  cursor: pointer;\n  z-index: 1;\n}\n\n.tagbox-button:hover {\n  background-color: var(--background);\n}\n\n.tagbox-data {\n  display: none;\n  width: 200px;\n  max-height: 200px;\n  box-sizing: border-box;\n  box-shadow: 0.5px 0.5px 1px 1px rgb(222, 222, 222);\n  border-radius: 4px;\n  padding-top: 4px;\n  padding-bottom: 4px;\n  position: absolute;\n  top: 40px;\n  overflow-y: scroll;\n  overflow-x: hidden;\n  background-color: var(--white-background);\n  z-index: 1;\n  outline: none;\n}\n\n.tagbox-item {\n  width: 200px;\n  height: 32px;\n  box-sizing: border-box;\n  cursor: pointer;\n  padding: 0 16px;\n  display: flex;\n  align-items: center;\n}\n\n.tagbox-item-selected {\n  background-color: var(--grid-line-focus-hover);\n}\n\n.tagbox-item-content {\n  margin-left: 12px;\n}\n\n.tagbox-item:hover {\n  background-color: var(--grid-line-focus-hover);\n}\n\n/* custom scroll*/\n/* width */\n::-webkit-scrollbar {\n  width: 4px;\n  height: 4px;\n  box\n}\n\n/* Track */\n::-webkit-scrollbar-track {\n  border-radius: 4px;\n}\n\n/* Handle */\n::-webkit-scrollbar-thumb {\n  background: var(--scroll-color);\n  border-radius: 4px;\n}\n/* hover */\n::-webkit-scrollbar-thumb:hover {\n  background: var(--scroll-hover-color);\n}\n</style>"]},"metadata":{},"sourceType":"module"}