{"ast":null,"code":"import BaseTablePaging from \"../base/BaseTablePaging.vue\";\nimport BaseButton from \"../base/BaseButton.vue\"; // import BaseInput from \"../base/BaseInput.vue\";\n\nimport DialogStaffDetail from \"../dialog/DialogStaffDetail.vue\";\nimport BaseCheckbox from \"../base/BaseCheckbox.vue\";\nimport BaseLoader from \"../base/BaseLoader.vue\";\nimport BaseToast from \"../base/BaseToast.vue\";\nimport BasePopup from \"../base/BasePopup.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    BaseLoader,\n    BaseTablePaging,\n    BaseButton,\n    // BaseInput,\n    DialogStaffDetail,\n    BaseCheckbox,\n    BaseToast,\n    BasePopup\n  },\n\n  created() {\n    this.loadData(50, 1);\n  },\n\n  props: [],\n\n  data() {\n    return {\n      isRowCheckboxSelected: false,\n      isHideLoader: false,\n      isShowDialog: false,\n      isHideToast: true,\n      isHidePopup: true,\n      staffs: {},\n      staffSelected: {},\n      staffSelectedId: null,\n      btnSaveMode: 0,\n      totalResult: null,\n      totalPage: null,\n      currentPage: null,\n      pageSize: null,\n      pageNumber: null,\n      inputSearch: null,\n      textSearch: null\n    };\n  },\n\n  // watch: {\n  //   inputSearch: function(value) {\n  //     console.log(value);\n  //   }  \n  // },\n  computed: {},\n  methods: {\n    searchByText() {},\n\n    /**\r\n     * thực hiện tải dữ liệu thông tin giáo viên và hiển thị trên bản\r\n     * Author: VQBao - 21/7/2022\r\n     */\n    loadData(pageSize, pageNumber) {\n      this.pageSize = pageSize;\n      this.pageNumber = pageNumber;\n      var baseURL = \"https://cukcuk.manhnv.net/api/v1/Employees/filter\";\n      this.isHideLoader = false;\n\n      try {\n        var me = this; // lấy dữ liệu từ api\n\n        axios.get(baseURL + \"?pageSize=\" + this.pageSize + \"&pageNumber=\" + this.pageNumber).then(function (respond) {\n          // ẩn loading\n          me.isHideLoader = true;\n          me.staffs = respond.data.Data;\n          me.totalResult = respond.data.TotalRecord;\n          me.totalPage = respond.data.TotalPage;\n          me.currentPage = respond.data.CurrentPage;\n        }).catch(function (res) {\n          console.log(res);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    /**\r\n     * khi bấm thêm thì form chi tiết giáo viên hiện ra\r\n     * Author: VQBao - 21/7/2022\r\n     */\n    addStaffOnClick() {\n      // btnSaveMode = 1 => thực hiện thêm mới\n      this.btnSaveMode = 1;\n      this.showStaffDialog(true);\n      this.staffSelected = {};\n      this.staffSelectedId = null;\n      var me = this; // gọi api lấy mã nhân viên mới\n\n      axios.get(\"https://cukcuk.manhnv.net/api/v1/Employees/NewEmployeeCode\").then(response => {\n        me.staffSelected.EmployeeCode = response.data;\n      }).catch(() => {\n        console.error(\"Lấy mã nhân viên mới không thành công\");\n      });\n    },\n\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của form thêm mới\r\n     * @param {Boolean} iShow - true: hiển thị, false: ẩn đi\r\n     * Author: VQBao - 21/7/2022\r\n     */\n    showStaffDialog(isShow) {\n      this.isShowDialog = isShow;\n    },\n\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của toast mesage\r\n     * Author: VQBao - 23/7/2022\r\n     */\n    hideToastMessage(isHide) {\n      this.isHideToast = isHide;\n    },\n\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của popup thông báo\r\n     * Author: VQBao - 23/7/2022\r\n     */\n    hidePopup(isHide) {\n      this.isHidePopup = isHide;\n    },\n\n    /**\r\n     * Khi nhấn vào tên hoặc icon sửa trong bảng, hiện form thông tin chi tiết\r\n     * Author: VQBao - 21/7/2022\r\n     */\n    rowOnClick(staff) {\n      // btnSaveMode = 0 => thực hiện sửa\n      this.btnSaveMode = 0;\n      this.staffSelected = staff;\n      this.staffSelectedId = staff.EmployeeId;\n      this.showStaffDialog(true);\n    },\n\n    /**\r\n     * khi bấm vào icon xóa thì thực hiện xóa\r\n     * Author: VQBao - 21/7/2022\r\n     */\n    removeStaffOnClick(staff) {\n      var me = this;\n      me.isHideLoader = false;\n      me.staffSelected = staff;\n      me.staffSelectedId = staff.EmployeeId;\n      me.hidePopup(false);\n      axios.delete(`https://cukcuk.manhnv.net/api/v1/Employees/${me.staffSelectedId}`).then(() => {\n        me.loadData();\n        me.hideToastMessage(false);\n        setTimeout(function () {\n          me.hideToastMessage(true);\n        }, 3000);\n      }).catch(() => {\n        console.log(\"Xóa lỗi\");\n      });\n    },\n\n    /**\r\n     * Thực hiện lấy object giáo viên khi tích chọn checkbox\r\n     * Author: VQBao - 22/7/2022\r\n     */\n    rowCheckboxSelected(staff) {\n      this.staffSelected = staff;\n      this.staffSelectedId = staff.EmployeeId; // this.isRowCheckboxSelected = true;\n\n      console.log(this.staffSelected);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8GA,OAAOA,eAAP,MAA4B,6BAA5B;AACA,OAAOC,UAAP,MAAuB,wBAAvB,C,CACA;;AACA,OAAOC,iBAAP,MAA8B,iCAA9B;AACA,OAAOC,YAAP,MAAyB,0BAAzB;AACA,OAAOC,UAAP,MAAuB,wBAAvB;AACA,OAAOC,SAAP,MAAsB,uBAAtB;AACA,OAAOC,SAAP,MAAsB,uBAAtB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,UAAU,EAAE;IACVJ,UADU;IAEVJ,eAFU;IAGVC,UAHU;IAIV;IACAC,iBALU;IAMVC,YANU;IAOVE,SAPU;IAQVC;EARU,CADC;;EAWbG,OAAO,GAAG;IACR,KAAKC,QAAL,CAAc,EAAd,EAAkB,CAAlB;EACD,CAbY;;EAcbC,KAAK,EAAE,EAdM;;EAebC,IAAI,GAAG;IACL,OAAO;MACLC,qBAAqB,EAAE,KADlB;MAELC,YAAY,EAAE,KAFT;MAGLC,YAAY,EAAE,KAHT;MAILC,WAAW,EAAE,IAJR;MAKLC,WAAW,EAAE,IALR;MAMLC,MAAM,EAAE,EANH;MAOLC,aAAa,EAAE,EAPV;MAQLC,eAAe,EAAE,IARZ;MASLC,WAAW,EAAE,CATR;MAULC,WAAW,EAAE,IAVR;MAWLC,SAAS,EAAE,IAXN;MAYLC,WAAW,EAAE,IAZR;MAaLC,QAAQ,EAAE,IAbL;MAcLC,UAAU,EAAE,IAdP;MAeLC,WAAW,EAAE,IAfR;MAgBLC,UAAU,EAAE;IAhBP,CAAP;EAkBD,CAlCY;;EAmCb;EACA;EACA;EACA;EACA;EACAC,QAAQ,EAAE,EAxCG;EA2CbC,OAAO,EAAE;IACPC,YAAY,GAAG,CAAE,CADV;;IAEP;;;;IAIArB,QAAQ,CAACe,QAAD,EAAWC,UAAX,EAAuB;MAC7B,KAAKD,QAAL,GAAgBA,QAAhB;MACA,KAAKC,UAAL,GAAkBA,UAAlB;MACA,IAAIM,OAAM,GAAI,mDAAd;MA