IAqHP;;;;IAIAgB,mBAAmB,CAACL,KAAD,EAAQ;MACzB,KAAKpC,aAAL,GAAqBoC,KAArB;MACA,KAAKnC,eAAL,GAAuBmC,KAAK,CAACC,UAA7B,CAFyB,CAGzB;;MACAb,OAAO,CAACC,GAAR,CAAY,KAAKzB,aAAjB;IACD;;EA9HM;AA3CI,CAAf","names":["BaseTablePaging","BaseButton","DialogStaffDetail","BaseCheckbox","BaseLoader","BaseToast","BasePopup","axios","components","created","loadData","props","data","isRowCheckboxSelected","isHideLoader","isShowDialog","isHideToast","isHidePopup","staffs","staffSelected","staffSelectedId","btnSaveMode","totalResult","totalPage","currentPage","pageSize","pageNumber","inputSearch","textSearch","computed","methods","searchByText","baseURL","me","get","then","respond","Data","TotalRecord","TotalPage","CurrentPage","catch","res","console","log","error","addStaffOnClick","showStaffDialog","response","EmployeeCode","isShow","hideToastMessage","isHide","hidePopup","rowOnClick","staff","EmployeeId","removeStaffOnClick","delete","setTimeout","rowCheckboxSelected"],"sourceRoot":"","sources":["E:\\MisaProject\\W2022_06\\misa_emis\\src\\components\\page\\StaffList.vue"],"sourcesContent":["<template>\r\n  <BaseLoader :isHideLoader=\"isHideLoader\" />\r\n  <div class=\"content-toolbar\">\r\n    <input\r\n      type=\"text\"\r\n      class=\"toolbar-input icon-right icon-Search\"\r\n      placeholder=\"Nhập Số hiệu cán bộ hoặc Tên cán bộ để tìm kiếm,...\"\r\n      v-model=\"inputTextSearch\"\r\n      @keyup=\"searchByText\"\r\n    />\r\n    <div class=\"toolbar-button\">\r\n      <BaseButton\r\n        id=\"btnCreate\"\r\n        buttonName=\"Thêm\"\r\n        buttonType=\"button-green\"\r\n        @click=\"addStaffOnClick\"\r\n      />\r\n      <BaseButton\r\n        id=\"btnExport\"\r\n        buttonName=\"Xuất khẩu\"\r\n        buttonType=\"button-white\"\r\n      />\r\n      <div id=\"btnMore\" class=\"toolbar-more icon-center icon-More\"></div>\r\n    </div>\r\n  </div>\r\n  <div class=\"page-table\">\r\n    <div class=\"content-table\">\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th class=\"col-checkbox\" style=\"width: 28px\">\r\n              <BaseCheckbox />\r\n            </th>\r\n            <th style=\"min-width: 100px\">Số hiệu cán bộ</th>\r\n            <th style=\"min-width: 120px\">Họ và tên</th>\r\n            <th style=\"min-width: 40px\">Số điện thoại</th>\r\n            <th style=\"min-width: 95px\">Tổ chuyên môn</th>\r\n            <th style=\"min-width: 80px\">QL theo môn</th>\r\n            <th style=\"min-width: 110px\">QL kho, phòng</th>\r\n            <th style=\"min-width: 85px\">Đào tạo QLTB</th>\r\n            <th style=\"min-width: 85px\">Đang làm việc</th>\r\n            <th style=\"min-width: 24px\"></th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"staff in staffs\" :key=\"staff.employeeId\">\r\n            <td class=\"text-align-center col-checkbox\">\r\n              <BaseCheckbox @click=\"rowCheckboxSelected(staff)\" />\r\n            </td>\r\n            <td class=\"text-align-center\">{{ staff.EmployeeCode }}</td>\r\n            <td\r\n              class=\"text-align-left col-full-name\"\r\n              style=\"color: var(--button-green-text)\"\r\n              @click=\"rowOnClick(staff)\"\r\n            >\r\n              {{ staff.FullName }}\r\n            </td>\r\n            <td class=\"text-align-center\">{{ staff.PhoneNumber }}</td>\r\n            <td class=\"text-align-left\">Tổ Toán-Tin</td>\r\n            <td class=\"text-align-left\">Lý</td>\r\n            <td class=\"text-align-left\">Kho phòng chung</td>\r\n            <td class=\"text-align-center col-check-icon\">\r\n              <div class=\"cell-check icon-center icon-check\"></div>\r\n            </td>\r\n            <td class=\"text-align-center col-check-icon\">\r\n              <div class=\"cell-check icon-center icon-check\"></div>\r\n            </td>\r\n            <td class=\"text-align-center col-icon\">\r\n              <div\r\n                class=\"cell-edit-icon icon-center icon-edit\"\r\n                @click=\"rowOnClick(staff)\"\r\n              ></div>\r\n              <div\r\n                class=\"cell-remove-icon icon-center icon-remove\"\r\n                @click=\"removeStaffOnClick(staff)\"\r\n              ></div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <BaseTablePaging\r\n      :totalResult=\"totalResult\"\r\n      :totalPage=\"totalPage\"\r\n      :currentPage=\"currentPage\"\r\n      @loadData=\"loadData\"\r\n    />\r\n  </div>\r\n  <DialogStaffDetail\r\n    :isShow=\"isShowDialog\"\r\n    :staffSelected=\"staffSelected\"\r\n    :staffSelectedId=\"staffSelectedId\"\r\n    :btnSaveMode=\"btnSaveMode\"\r\n    @isShowStaffDialog=\"showStaffDialog\"\r\n    @loadData=\"loadData\"\r\n  />\r\n  <BaseToast\r\n    toastType=\"success\"\r\n    :isHideToast=\"isHideToast\"\r\n    title=\"Thành công\"\r\n    text=\"Xóa dữ liệu thành công\"\r\n  />\r\n  <BasePopup\r\n    title=\"Thông báo\"\r\n    text=\"Bạn có chắc chắn muốn xóa thông tin giáo viên đang chọn không?\"\r\n    :isHidePopup=\"isHidePopup\"\r\n    @hidePopup=\"hidePopup\"\r\n  />\r\n</template> \r\n<script>\r\nimport BaseTablePaging from \"../base/BaseTablePaging.vue\";\r\nimport BaseButton from \"../base/BaseButton.vue\";\r\n// import BaseInput from \"../base/BaseInput.vue\";\r\nimport DialogStaffDetail from \"../dialog/DialogStaffDetail.vue\";\r\nimport BaseCheckbox from \"../base/BaseCheckbox.vue\";\r\nimport BaseLoader from \"../base/BaseLoader.vue\";\r\nimport BaseToast from \"../base/BaseToast.vue\";\r\nimport BasePopup from \"../base/BasePopup.vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  components: {\r\n    BaseLoader,\r\n    BaseTablePaging,\r\n    BaseButton,\r\n    // BaseInput,\r\n    DialogStaffDetail,\r\n    BaseCheckbox,\r\n    BaseToast,\r\n    BasePopup,\r\n  },\r\n  created() {\r\n    this.loadData(50, 1);\r\n  },\r\n  props: [],\r\n  data() {\r\n    return {\r\n      isRowCheckboxSelected: false,\r\n      isHideLoader: false,\r\n      isShowDialog: false,\r\n      isHideToast: true,\r\n      isHidePopup: true,\r\n      staffs: {},\r\n      staffSelected: {},\r\n      staffSelectedId: null,\r\n      btnSaveMode: 0,\r\n      totalResult: null,\r\n      totalPage: null,\r\n      currentPage: null,\r\n      pageSize: null,\r\n      pageNumber: null,\r\n      inputSearch: null,\r\n      textSearch: null,\r\n    };\r\n  },\r\n  // watch: {\r\n  //   inputSearch: function(value) {\r\n  //     console.log(value);\r\n  //   }  \r\n  // },\r\n  computed: {\r\n    \r\n  },\r\n  methods: {\r\n    searchByText() {},\r\n    /**\r\n     * thực hiện tải dữ liệu thông tin giáo viên và hiển thị trên bản\r\n     * Author: VQBao - 21/7/2022\r\n     */\r\n    loadData(pageSize, pageNumber) {\r\n      this.pageSize = pageSize;\r\n      this.pageNumber = pageNumber;\r\n      var baseURL = \"https://cukcuk.manhnv.net/api/v1/Employees/filter\";\r\n      this.isHideLoader = false;\r\n      try {\r\n        var me = this;\r\n        // lấy dữ liệu từ api\r\n        axios\r\n          .get(\r\n            baseURL +\r\n              \"?pageSize=\" +\r\n              this.pageSize +\r\n              \"&pageNumber=\" +\r\n              this.pageNumber\r\n          )\r\n          .then(function (respond) {\r\n            // ẩn loading\r\n            me.isHideLoader = true;\r\n            me.staffs = respond.data.Data;\r\n            me.totalResult = respond.data.TotalRecord;\r\n            me.totalPage = respond.data.TotalPage;\r\n            me.currentPage = respond.data.CurrentPage;\r\n          })\r\n          .catch(function (res) {\r\n            console.log(res);\r\n          });\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    /**\r\n     * khi bấm thêm thì form chi tiết giáo viên hiện ra\r\n     * Author: VQBao - 21/7/2022\r\n     */\r\n\r\n    addStaffOnClick() {\r\n      // btnSaveMode = 1 => thực hiện thêm mới\r\n      this.btnSaveMode = 1;\r\n      this.showStaffDialog(true);\r\n      this.staffSelected = {};\r\n      this.staffSelectedId = null;\r\n      var me = this;\r\n      // gọi api lấy mã nhân viên mới\r\n      axios\r\n        .get(\"https://cukcuk.manhnv.net/api/v1/Employees/NewEmployeeCode\")\r\n        .then((response) => {\r\n          me.staffSelected.EmployeeCode = response.data;\r\n        })\r\n        .catch(() => {\r\n          console.error(\"Lấy mã nhân viên mới không thành công\");\r\n        });\r\n    },\r\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của form thêm mới\r\n     * @param {Boolean} iShow - true: hiển thị, false: ẩn đi\r\n     * Author: VQBao - 21/7/2022\r\n     */\r\n    showStaffDialog(isShow) {\r\n      this.isShowDialog = isShow;\r\n    },\r\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của toast mesage\r\n     * Author: VQBao - 23/7/2022\r\n     */\r\n    hideToastMessage(isHide) {\r\n      this.isHideToast = isHide;\r\n    },\r\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của popup thông báo\r\n     * Author: VQBao - 23/7/2022\r\n     */\r\n    hidePopup(isHide) {\r\n      this.isHidePopup = isHide;\r\n    },\r\n    /**\r\n     * Khi nhấn vào tên hoặc icon sửa trong bảng, hiện form thông tin chi tiết\r\n     * Author: VQBao - 21/7/2022\r\n     */\r\n    rowOnClick(staff) {\r\n      // btnSaveMode = 0 => thực hiện sửa\r\n      this.btnSaveMode = 0;\r\n      this.staffSelected = staff;\r\n      this.staffSelectedId = staff.EmployeeId;\r\n      this.showStaffDialog(true);\r\n    },\r\n    /**\r\n     * khi bấm vào icon xóa thì thực hiện xóa\r\n     * Author: VQBao - 21/7/2022\r\n     */\r\n    removeStaffOnClick(staff) {\r\n      var me = this;\r\n      me.isHideLoader = false;\r\n      me.staffSelected = staff;\r\n      me.staffSelectedId = staff.EmployeeId;\r\n      me.hidePopup(false);\r\n      axios\r\n        .delete(\r\n          `https://cukcuk.manhnv.net/api/v1/Employees/${me.staffSelectedId}`\r\n        )\r\n        .then(() => {\r\n          me.loadData();\r\n          me.hideToastMessage(false);\r\n          setTimeout(function () {\r\n            me.hideToastMessage(true);\r\n          }, 3000);\r\n        })\r\n        .catch(() => {\r\n          console.log(\"Xóa lỗi\");\r\n        });\r\n    },\r\n    /**\r\n     * Thực hiện lấy object giáo viên khi tích chọn checkbox\r\n     * Author: VQBao - 22/7/2022\r\n     */\r\n    rowCheckboxSelected(staff) {\r\n      this.staffSelected = staff;\r\n      this.staffSelectedId = staff.EmployeeId;\r\n      // this.isRowCheckboxSelected = true;\r\n      console.log(this.staffSelected);\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\ninput {\r\n    min-width: 120px;\r\n    height: 32px;\r\n    outline: none;\r\n    border: 1px solid #cccccc;\r\n    border-radius: 4px;\r\n    box-sizing: border-box;\r\n    padding: 0 32px 0 16px;\r\n}\r\n\r\ninput:focus {\r\n    border-color: #08BF1E;\r\n}\r\n\r\ninput::placeholder {\r\n    color: var(--button-grey-text);\r\n}\r\n.content-toolbar {\r\n  height: 64px;\r\n  width: 100%;\r\n  position: relative;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.toolbar-button {\r\n  box-sizing: border-box;\r\n  display: flex;\r\n  align-items: center;\r\n  position: absolute;\r\n  right: 16px;\r\n}\r\n\r\n.toolbar-button button {\r\n  margin-left: 8px;\r\n}\r\n\r\n.toolbar-more {\r\n  background-color: #ffffff;\r\n  width: 32px;\r\n  height: 32px;\r\n  background-size: 28px 28px;\r\n  margin-left: 8px;\r\n  border-radius: 4px;\r\n  border: 1px solid;\r\n  border-color: var(--button-border);\r\n  box-sizing: border-box;\r\n  cursor: pointer;\r\n}\r\n\r\n.page-table {\r\n  position: relative;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-content: stretch;\r\n  flex: 1;\r\n  justify-content: space-between;\r\n  height: calc(100vh - 112px);\r\n  width: 100%;\r\n}\r\n\r\n.content-table {\r\n  max-width: 100%;\r\n  flex: 1;\r\n  flex-basis: 100px;\r\n  box-sizing: border-box;\r\n  heigh