{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  props: {\n    url: String,\n    propKey: String,\n    propValue: String\n  },\n\n  data() {\n    return {\n      data: [],\n      isShowListData: false,\n      isShowTag: [],\n      selectedItem: null,\n      selectedItemList: [],\n      isCheck: [],\n      isCheckAll: false\n    };\n  },\n\n  created() {\n    var me = this;\n    /**\n     * Thực hiện lấy dữ liệu từ api\n     * Author: VQBao - 10/8/2022\n     */\n\n    axios.get(me.url).then(res => {\n      me.data = res.data;\n      me.selectedItem = res.data[1][me.propValue];\n    }).catch(err => {\n      console.error(err);\n    });\n  },\n\n  methods: {\n    /**\n     * Thực hiện ẩn hiện danh sách các select\n     * Author: VQBao - 10/8/2022\n     */\n    showListData() {\n      this.isShowListData = !this.isShowListData;\n    },\n\n    /**\n     * Thực hiện chọn item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectDataItem(item, index) {\n      this.selectedItem = item[this.propValue];\n\n      if (!this.isCheck[index]) {\n        this.isCheck[index] = true;\n        this.isShowTag[index] = true;\n        this.selectedItemList.push(this.selectedItem);\n\n        if (this.selectedItemList.length == this.data.length) {\n          this.isCheckAll = true;\n        }\n      } else {\n        this.isCheck[index] = false;\n        this.isShowTag[index] = false;\n        let id = this.selectedItemList.indexOf(this.selectedItem);\n        this.selectedItemList.splice(id, 1);\n        this.isCheckAll = false;\n      }\n    },\n\n    /**\n     * Thực hiện chọn tất cả item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectAllItem() {\n      if (!this.isCheckAll) {\n        this.isCheckAll = true;\n        this.selectedItemList = [];\n\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = true;\n          let item = this.data[i];\n          this.selectedItemList.push(item[this.propValue]);\n        }\n      } else {\n        this.isCheckAll = false;\n\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = false;\n        }\n\n        this.selectedItemList = [];\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,KAAK,EAAE;IACLC,GAAG,EAAEC,MADA;IAELC,OAAO,EAAED,MAFJ;IAGLE,SAAS,EAAEF;EAHN,CADM;;EAMbG,IAAI,GAAG;IACL,OAAO;MACLA,IAAI,EAAE,EADD;MAELC,cAAc,EAAE,KAFX;MAGLC,SAAS,EAAE,EAHN;MAILC,YAAY,EAAE,IAJT;MAKLC,gBAAgB,EAAE,EALb;MAMLC,OAAO,EAAE,EANJ;MAOLC,UAAU,EAAE;IAPP,CAAP;EASD,CAhBY;;EAiBbC,OAAO,GAAG;IACR,IAAIC,EAAC,GAAI,IAAT;IACA;;;;;IAIAd,KAAI,CACDe,GADH,CACOD,EAAE,CAACZ,GADV,EAEGc,IAFH,CAESC,GAAD,IAAS;MACbH,EAAE,CAACR,IAAH,GAAUW,GAAG,CAACX,IAAd;MACAQ,EAAE,CAACL,YAAH,GAAkBQ,GAAG,CAACX,IAAJ,CAAS,CAAT,EAAYQ,EAAE,CAACT,SAAf,CAAlB;IACD,CALH,EAMGa,KANH,CAMUC,GAAD,IAAS;MACdC,OAAO,CAACC,KAAR,CAAcF,GAAd;IACD,CARH;EASD,CAhCY;;EAiCbG,OAAO,EAAE;IACP;;;;IAIAC,YAAY,GAAG;MACb,KAAKhB,cAAL,GAAsB,CAAC,KAAKA,cAA5B;IACD,CAPM;;IAQP;;;;IAIAiB,cAAc,CAACC,IAAD,EAAOC,KAAP,EAAc;MAC1B,KAAKjB,YAAL,GAAoBgB,IAAI,CAAC,KAAKpB,SAAN,CAAxB;;MACA,IAAI,CAAC,KAAKM,OAAL,CAAae,KAAb,CAAL,EAA0B;QACxB,KAAKf,OAAL,CAAae,KAAb,IAAsB,IAAtB;QACA,KAAKlB,SAAL,CAAekB,KAAf,IAAwB,IAAxB;QACA,KAAKhB,gBAAL,CAAsBiB,IAAtB,CAA2B,KAAKlB,YAAhC;;QACA,IAAI,KAAKC,gBAAL,CAAsBkB,MAAtB,IAAgC,KAAKtB,IAAL,CAAUsB,MAA9C,EAAsD;UACpD,KAAKhB,UAAL,GAAkB,IAAlB;QACF;MACF,CAPA,MAOO;QACL,KAAKD,OAAL,CAAae,KAAb,IAAsB,KAAtB;QACA,KAAKlB,SAAL,CAAekB,KAAf,IAAwB,KAAxB;QACA,IAAIG,EAAC,GAAI,KAAKnB,gBAAL,CAAsBoB,OAAtB,CAA8B,KAAKrB,YAAnC,CAAT;QACA,KAAKC,gBAAL,CAAsBqB,MAAtB,CAA6BF,EAA7B,EAAiC,CAAjC;QACA,KAAKjB,UAAL,GAAkB,KAAlB;MACF;IACD,CA5BM;;IA6BP;;;;IAIAoB,aAAa,GAAG;MACd,IAAI,CAAC,KAAKpB,UAAV,EAAsB;QACpB,KAAKA,UAAL,GAAkB,IAAlB;QACA,KAAKF,gBAAL,GAAwB,EAAxB;;QACA,KAAK,IAAIuB,IAAI,CAAb,EAAgBA,IAAI,KAAK3B,IAAL,CAAUsB,MAA9B,EAAsCK,CAAC,EAAvC,EAA2C;UACzC,KAAKtB,OAAL,CAAasB,CAAb,IAAkB,IAAlB;UACA,IAAIR,IAAG,GAAI,KAAKnB,IAAL,CAAU2B,CAAV,CAAX;UACA,KAAKvB,gBAAL,CAAs