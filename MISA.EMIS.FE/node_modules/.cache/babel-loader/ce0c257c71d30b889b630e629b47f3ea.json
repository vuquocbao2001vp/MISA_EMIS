x)\"\n        :class=\"{ 'tagbox-item-selected': isCheck[index] }\"\n      >\n        <BaseCheckbox\n          :class=\"isCheck[index] ? 'checkbox-active' : 'checkbox-inactive'\"\n        />\n        <div class=\"tagbox-item-content\">{{ item[this.propValue] }}</div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  props: {\n    url: String,\n    propKey: String,\n    propValue: String,\n  },\n  data() {\n    return {\n      data: [],\n      isShowListData: false,\n      isShowTag: [],\n      selectedItem: null,\n      selectedItemList: [],\n      isCheck: [],\n      isCheckAll: false,\n    };\n  },\n  created() {\n    var me = this;\n    /**\n     * Thực hiện lấy dữ liệu từ api\n     * Author: VQBao - 10/8/2022\n     */\n    axios\n      .get(me.url)\n      .then((res) => {\n        me.data = res.data;\n        me.selectedItem = res.data[1][me.propValue];\n      })\n      .catch((err) => {\n        console.error(err);\n      });\n  },\n  methods: {\n    /**\n     * Thực hiện ẩn hiện danh sách các select\n     * Author: VQBao - 10/8/2022\n     */\n    showListData() {\n      this.isShowListData = !this.isShowListData;\n    },\n    /**\n     * Thực hiện chọn item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectDataItem(item, index) {\n      this.selectedItem = item[this.propValue];\n      if (!this.isCheck[index]) {\n        this.isCheck[index] = true;\n        this.isShowTag[index] = true;\n        this.selectedItemList.push(this.selectedItem);\n        if (this.selectedItemList.length == this.data.length) {\n          this.isCheckAll = true;\n        }\n      } else {\n        this.isCheck[index] = false;\n        this.isShowTag[index] = false;\n        let id = this.selectedItemList.indexOf(this.selectedItem);\n        this.selectedItemList.splice(id, 1);\n        this.isCheckAll = false;\n      }\n    },\n    /**\n     * Thực hiện chọn tất cả item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectAllItem() {\n      if (!this.isCheckAll) {\n        this.isCheckAll = true;\n        this.selectedItemList = [];\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = true;\n          let item = this.data[i];\n          this.selectedItemList.push(item[this.propValue]);\n        }\n      } else {\n        this.isCheckAll = false;\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = false;\n        }\n        this.selectedItemList = [];\n      }\n    },\n  },\n};\n</script>\n<style scoped>\n.is-show-tag {\n  display: flex !important;\n}\n\n.selected-item-tag {\n  position: relative;\n  min-width: fit-content;\n  height: 24px;\n  background-color: var(--button-grey-background);\n  display: none;\n  align-items: center;\n  padding-left: 8px;\n  border-radius: 4px;\n  margin-right: 8px;\n  box-sizing: border-box;\n}\n\n.selected-item-tag-icon {\n  height: 24px;\n  width: 24px;\n  cursor: pointer;\n  box-sizing: border-box;\n}\n\n.is-show {\n  display: block !important;\n}\n.tagbox {\n  position: relative;\n  width: 200px;\n  box-sizing: border-box;\n  display: flex;\n  flex-direction: column;\n}\n.tagbox-content {\n  width: 200px;\n  height: 32px;\n  border: 1px solid;\n  border-radius: 4px;\n  border-color: var(--button-border);\n  box-sizing: border-box;\n  padding: 0 4px;\n}\n\n.tagbox-content-text {\n  height: 32px;\n  width: 168px;\n  display: flex;\n  align-items: center;\n  box-sizing: border-box;\n  overflow-x: scroll;\n  overflow-y: hidden;\n}\n\n.tagbox-button {\n  width: 32px;\n  height: 32px;\n  position: absolute;\n  right: 0px;\n  top: 0px;\n  cursor: pointer;\n  z-index: 1;\n}\n\n.tagbox-button:hover {\n  background-color: var(--background);\n}\n\n.tagbox-data {\n  display: none;\n  width: 200px;\n  max-height: 200px;\n  box-sizing: border-box;\n  box-shadow: 0.5px 0.5px 1px 1px rgb(222, 222, 222);\n  border-radius: 4px;\n  padding-top: 4px;\n  padding-bottom: 4px;\n  position: absolute;\n  top: 40px;\n  overflow-y: scroll;\n  overflow-x: hidden;\n  background-color: var(--white-background);\n  z-index: 1;\n  outline: none;\n}\n\n.tagbox-item {\n  width: 200px;\n  height: 32px;\n  box-sizing: border-box;\n  cursor: pointer;\n  padding: 0 16px;\n  display: flex;\n  align-items: center;\n}\n\n.tagbox-item-selected {\n  background-color: var(--grid-line-focus-hover);\n}\n\n.tagbox-item-content {\n  margin-left: 12px;\n}\n\n.tagbox-item:hover {\n  background-color: var(--grid-line-focus-hover);\n}\n\n/* custom scroll*/\n/* width */\n::-webkit-scrollbar {\n  width: 4px;\n  height: 4px;\n}\n\n/* Track */\n::-webkit-scrollbar-track {\n  border-radius: 4px;\n}\n\n/* Handle */\n::-webkit-scrollbar-thumb {\n  background: var(--scroll-color);\n  border-radius: 4px;\n}\n/* hover */\n::-webkit-scrollbar-thumb:hover {\n  background: var(--scroll-hover-color);\n}\n</style>"]},"metadata":{},"sourceType":"module"}