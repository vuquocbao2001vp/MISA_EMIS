watch","value","console","log","error","methods","searchByText","clearTimeout","self","setTimeout","baseURL","me","get","then","respond","Data","TotalRecord","TotalPage","CurrentPage","i","length","catch","res","addStaffOnClick","EditMode","Add","showStaffDialog","response","EmployeeCode","isShow","hideToastMessage","isHide","hidePopup","rowOnClick","staff","Edit","EmployeeId","removeStaffOnClick","delete","rowCheckboxSelected","index","push","id","indexOf","splice","removeStaffsOnClick","checkAllCheckbox"],"sourceRoot":"","sources":["E:\\MisaProject\\W2022_06\\misa_emis\\src\\components\\page\\StaffList.vue"],"sourcesContent":["<template>\r\n  <BaseLoader :isHideLoader=\"isHideLoader\" />\r\n  <div class=\"content-toolbar\">\r\n    <input\r\n      type=\"text\"\r\n      class=\"toolbar-input\"\r\n      placeholder=\"Nhập Số hiệu cán bộ hoặc Tên cán bộ để tìm kiếm,...\"\r\n      v-model=\"inputTextSearch\"\r\n      @keyup=\"searchByText\"\r\n    />\r\n    <div class=\"input-search icon-right icon-Search\"></div>\r\n    <div class=\"toolbar-button\">\r\n      <BaseButton\r\n        id=\"btnCreate\"\r\n        buttonName=\"Thêm\"\r\n        buttonType=\"button-green\"\r\n        @click=\"addStaffOnClick\"\r\n      />\r\n      <BaseButton\r\n        id=\"btnExport\"\r\n        buttonName=\"Xuất khẩu\"\r\n        buttonType=\"button-white\"\r\n      />\r\n      <div\r\n        id=\"btnMore\"\r\n        class=\"toolbar-more icon-center icon-More\"\r\n        @click=\"removeStaffsOnClick\"\r\n      >\r\n      <BaseButton class=\"icon-left icon-delete\" id=\"btnDeleteMore\" buttonName=\"Xóa\" buttonType=\"button-white\"/></div>\r\n    </div>\r\n  </div>\r\n  <div class=\"page-table\">\r\n    <div class=\"content-table\">\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th class=\"col-checkbox\" style=\"width: 28px\">\r\n              <BaseCheckbox\r\n                @click=\"checkAllCheckbox\"\r\n                :class=\"isCheckAll ? 'checkbox-active' : 'checkbox-inactive'\"\r\n              />\r\n            </th>\r\n            <th style=\"min-width: 100px\">Số hiệu cán bộ</th>\r\n            <th style=\"min-width: 120px\">Họ và tên</th>\r\n            <th style=\"min-width: 40px\">Số điện thoại</th>\r\n            <th style=\"min-width: 95px\">Tổ chuyên môn</th>\r\n            <th style=\"min-width: 80px\">QL theo môn</th>\r\n            <th style=\"min-width: 110px\">QL kho, phòng</th>\r\n            <th style=\"min-width: 85px\">Đào tạo QLTB</th>\r\n            <th style=\"min-width: 85px\">Đang làm việc</th>\r\n            <th style=\"min-width: 24px\"></th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr\r\n            v-for=\"(staff, index) in staffs\"\r\n            :key=\"staff.employeeId\"\r\n            :class=\"isCheckCheckbox[index] ? 'row-selected' : ''\"\r\n          >\r\n            <td class=\"text-align-center col-checkbox\">\r\n              <BaseCheckbox\r\n                @click=\"rowCheckboxSelected(staff, index)\"\r\n                :class=\"isCheckCheckbox[index] ? 'checkbox-active' : 'checkbox-inactive'\"\r\n              />\r\n            </td>\r\n            <td class=\"text-align-center\">{{ staff.EmployeeCode }}</td>\r\n            <td\r\n              class=\"text-align-left col-full-name\"\r\n              style=\"color: var(--button-green-text)\"\r\n              @click=\"rowOnClick(staff)\"\r\n            >\r\n              {{ staff.FullName }}\r\n            </td>\r\n            <td class=\"text-align-center\">{{ staff.PhoneNumber }}</td>\r\n            <td class=\"text-align-left\">Tổ Toán-Tin</td>\r\n            <td class=\"text-align-left\">Lý</td>\r\n            <td class=\"text-align-left\">Kho phòng chung</td>\r\n            <td class=\"text-align-center col-check-icon\">\r\n              <div class=\"cell-check icon-center icon-check\"></div>\r\n            </td>\r\n            <td class=\"text-align-center col-check-icon\">\r\n              <div class=\"cell-check icon-center icon-check\"></div>\r\n            </td>\r\n            <td class=\"text-align-center col-icon\">\r\n              <div\r\n                class=\"cell-edit-icon icon-center icon-edit\"\r\n                @click=\"rowOnClick(staff)\"\r\n              ></div>\r\n              <div\r\n                class=\"cell-remove-icon icon-center icon-remove\"\r\n                @click=\"removeStaffOnClick(staff)\"\r\n              ></div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <BaseTablePaging\r\n      :totalResult=\"totalResult\"\r\n      :totalPage=\"totalPage\"\r\n      :currentPage=\"currentPage\"\r\n      @loadData=\"loadData\"\r\n    />\r\n  </div>\r\n  <DialogStaffDetail\r\n    :isShow=\"isShowDialog\"\r\n    :staffSelected=\"staffSelected\"\r\n    :staffSelectedId=\"staffSelectedId\"\r\n    :btnSaveMode=\"btnSaveMode\"\r\n    @isShowStaffDialog=\"showStaffDialog\"\r\n    @loadData=\"loadData\"\r\n  />\r\n  <BaseToast\r\n    toastType=\"success\"\r\n    :isHideToast=\"isHideToast\"\r\n    title=\"Thành công\"\r\n    text=\"Xóa dữ liệu thành công\"\r\n  />\r\n  <BasePopup\r\n    title=\"Thông báo\"\r\n    text=\"Bạn có chắc chắn muốn xóa thông tin giáo viên đang chọn không?\"\r\n    :isHidePopup=\"isHidePopup\"\r\n    @hidePopup=\"hidePopup\"\r\n  />\r\n</template> \r\n<script>\r\nimport BaseTablePaging from \"../base/BaseTablePaging.vue\";\r\nimport DialogStaffDetail from \"../dialog/DialogStaffDetail.vue\";\r\nimport BaseToast from \"../base/BaseToast.vue\";\r\nimport BasePopup from \"../base/BasePopup.vue\";\r\nimport axios from \"axios\";\r\nimport MISAEnum from '@/script/constants/MISAEnum';\r\n// import MISAResource from '@/scripts/constants/MISAResource';\r\n\r\nexport default {\r\n  components: {\r\n    BaseTablePaging,\r\n    DialogStaffDetail,\r\n    BaseToast,\r\n    BasePopup,\r\n  },\r\n  created() {\r\n    this.loadData(25, 1, \"\");\r\n  },\r\n  props: [],\r\n  data() {\r\n    return {\r\n      isHideLoader: false,\r\n      isShowDialog: false,\r\n      isHideToast: true,\r\n      isHidePopup: true,\r\n      staffs: {},\r\n      staffSelected: {},\r\n      staffSelectedId: null,\r\n      btnSaveMode: null,\r\n      totalResult: null,\r\n      totalPage: null,\r\n      currentPage: null,\r\n      pageSize: null,\r\n      pageNumber: null,\r\n      filterName: null,\r\n      inputTextSearch: null,\r\n      outputTextSearch: null,\r\n      timeout: null,\r\n      rowStaff: [],\r\n      isCheckCheckbox: [],\r\n      isCheckAll: false,\r\n    };\r\n  },\r\n  watch: {\r\n    /**\r\n     * khi người dùng nhập xong thì thực hiện gọi api lấy dữ liệu theo từ khóa tìm kiếm\r\n     * Author: VQBao - 27/7/2022\r\n     */\r\n    outputTextSearch: function (value) {\r\n      try {\r\n        console.log(value);\r\n        this.loadData(25, 1, value);\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    /**\r\n     * khi người dùng nhập vào ô search input, sau khi ngừng 500ms thì lấy ra giá trị vừa nhập xong\r\n     * Author: VQBao - 27/7/2022\r\n     */\r\n    searchByText() {\r\n      clearTimeout(this.timeout);\r\n      var self = this;\r\n      this.timeout = setTimeout(function () {\r\n        self.outputTextSearch = self.inputTextSearch;\r\n      }, 500);\r\n    },\r\n    /**\r\n     * thực hiện tải dữ liệu thông tin giáo viên và hiển thị trên bảng\r\n     * Author: VQBao - 21/7/2022\r\n     */\r\n    loadData(pageSize, pageNumber, filterName) {\r\n      this.pageSize = pageSize;\r\n      this.pageNumber = pageNumber;\r\n      this.filterName = filterName;\r\n      var baseURL = \"https://cukcuk.manhnv.net/api/v1/Employees/filter\";\r\n      this.isHideLoader = false;\r\n      var me = this;\r\n      // loadData khi chưa có thông tin tìm kiếm\r\n      if (filterName == \"\") {\r\n        try {\r\n          axios\r\n            .get(\r\n              baseURL +\r\n                \"?pageSize=\" +\r\n                me.pageSize +\r\n                \"&pageNumber=\" +\r\n                me.pageNumber\r\n            )\r\n            .then(function (respond) {\r\n              // ẩn loading\r\n              me.isHideLoader = true;\r\n              me.staffs = respond.data.Data;\r\n              me.totalResult = respond.data.TotalRecord;\r\n              me.totalPage = respond.data.TotalPage;\r\n              me.currentPage = respond.data.CurrentPage;\r\n              // ô checkbox ở các hàng đều bằng false\r\n              for (let i = 0; i < me.staffs.length; i++) {\r\n                me.isCheckCheckbox[i] = false;\r\n              }\r\n            })\r\n            .catch(function (res) {\r\n              console.log(res);\r\n            });\r\n        } catch (error) {\r\n          console.log(error);\r\n        }\r\n      }\r\n      // loadData khi có từ khóa tìm kiếm\r\n      else {\r\n        try {\r\n          axios\r\n            .get(\r\n              baseURL +\r\n                \"?pageSize=\" +\r\n                me.pageSize +\r\n                \"&pageNumber=\" +\r\n                me.pageNumber +\r\n                \"&employeeFilter=\" +\r\n                me.filterName\r\n            )\r\n            .then(function (respond) {\r\n              // ẩn loading\r\n              me.isHideLoader = true;\r\n              me.staffs = respond.data.Data;\r\n              me.totalResult = respond.data.TotalRecord;\r\n              me.totalPage = respond.data.TotalPage;\r\n              me.currentPage = respond.data.CurrentPage;\r\n            })\r\n            .catch(function (res) {\r\n              console.log(res);\r\n            });\r\n        } catch (error) {\r\n          console.log(error);\r\n        }\r\n      }\r\n    },\r\n    /**\r\n     * khi bấm thêm thì form chi tiết giáo viên hiện ra\r\n     * Author: VQBao - 21/7/2022\r\n     */\r\n\r\n    addStaffOnClick() {\r\n      // btnSaveMode = MISAEnum.EditMode.Ad => thực hiện thêm mới\r\n      this.btnSaveMode = MISAEnum.EditMode.Add;\r\n      this.showStaffDialog(true);\r\n      this.staffSelected = {};\r\n      this.staffSelectedId = null;\r\n      var me = this;\r\n      // gọi api lấy mã nhân viên mới\r\n      axios\r\n        .get(\"https://cukcuk.manhnv.net/api/v1/Employees/NewEmployeeCode\")\r\n        .then((response) => {\r\n          me.staffSelected.EmployeeCode = response.data;\r\n        })\r\n        .catch(() => {\r\n          console.error(\"Lấy mã nhân viên mới không thành công\");\r\n        });\r\n    },\r\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của form thêm mới\r\n     * @param {Boolean} iShow - true: hiển thị, false: ẩn đi\r\n     * Author: VQBao - 21/7/2022\r\n     */\r\n    showStaffDialog(isShow) {\r\n      this.isShowDialog = isShow;\r\n    },\r\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của toast mesage\r\n     * Author: VQBao - 23/7/2022\r\n     */\r\n    hideToastMessage(isHide) {\r\n      this.isHideToast = isHide;\r\n    },\r\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của popup thông báo\r\n     * Author: VQBao - 23/7/2022\r\n     */\r\n    hidePopup(isHide) {\r\n      this.isHidePopup = isHide;\r\n    },\r\n    /**\r\n     * Khi nhấn vào tên hoặc icon sửa trong bảng, hiện form thông tin chi tiết\r\n     * Author: VQBao - 21/7/2022\r\n     */\r\n    rowOnClick(staff) {\r\n      // btnSaveMode = MISAEnum.EditMode.Edit => thực hiện sửa\r\n      this.btnSaveMode = MISAEnum.EditMode.Edit;\r\n      this.staffSelected = staff;\r\n      this.staffSelectedId = staff.EmployeeId;\r\n      this.showStaffDialog(true);\r\n    },\r\n    /**\r\n     * khi bấm vào icon xóa thì thực hiện xóa dòng dữ liệu giáo viên đó\r\n     * Author: VQBao - 21/7/2022\r\n     */\r\n    removeStaffOnClick(staff) {\r\n      var me = this;\r\n      me.isHideLoader = false;\r\n      me.hidePopup(false);\r\n      axios\r\n        .delete(\r\n          `https://cukcuk.manhnv.net/api/v1/Employees/${staff.EmployeeId}`\r\n        )\r\n        .then(() => {\r\n          me.loadData(50, 1, \"\");\r\n          me.hideToastMessage(false);\r\n          setTimeout(function () {\r\n            me.hideToastMessage(true);\r\n          }, 3000);\r\n        })\r\n        .catch(() => {\r\n          console.log(\"Xóa lỗi\");\r\n        });\r\n    },\r\n    /**\r\n     * Thực hiện lấy object giáo viên khi tích chọn 1 hoặc nhiều checkbox\r\n     * Author: VQBao - 22/7/2022\r\n     */\r\n    rowCheckboxSelected(staff, index) {\r\n      this.staffSelected = staff;\r\n      this.staffSelectedId = staff.EmployeeId;\r\n      if (!this.isCheckCheckbox[index]) {\r\n        this.isCheckCheckbox[index] = true;\r\n        this.rowStaff.push(this.staffSelected);\r\n        if(this.rowStaff.length == this.staffs.length){\r\n          this.isCheckAll = true;\r\n        }\r\n      } else {\r\n        this.isCheckCheckbox[index] = false;\r\n        let id = this.rowStaff.indexOf(staff);\r\n        this.rowStaff.splice(id, 1);\r\n        if(this.rowStaff.length != this.staffs.length){\r\n          this.isCheckAll = false;\r\n        }\r\n      }\r\n    },\r\n    /**\r\n     * Thực hiện xóa hàng loạt\r\n     * Author: VQBao - 28/7/2022\r\n     */\r\n    removeStaffsOnClick() {\r\n      for (let i = 0; i < this.rowStaff.length; i++) {\r\n        this.removeStaffOnClick(this.rowStaff[i]);\r\n      }\r\n      this.rowStaff = [];\r\n    },\r\n    /**\r\n     * Thực hiện check all\r\n     * Author: VQBao - 28/7/2022\r\n     */\r\n    checkAllCheckbox() {\r\n      if (!this.isCheckAll) {\r\n        this.rowStaff = [];\r\n        this.isCheckAll = true;\r\n        for (let i = 0; i < this.staffs.length; i++) {\r\n          this.isCheckCheckbox[i] = true;\r\n          this.rowStaff.push(this.staffs[i]);\r\n        }\r\n      } else {\r\n        this.isCheckAll = false;\r\n        for (let i = 0; i < this.staffs.length; i++) {\r\n          this.isCheckCheckbox[i] = false;\r\n        }\r\n        this.rowStaff = [];\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\ninput {\r\n  min-width: 120px;\r\n  height: 32px;\r\n  outline: none;\r\n  border: 1px solid #cccccc;\r\n  border-radius: 4px;\r\n  box-sizing: border-box;\r\n  padding: 0 32px 0 16px;\r\n}\r\n\r\ninput:focus {\r\n  border-color: #08bf1e;\r\n}\r\n\r\ninput::placeholder {\r\n  color: var(--button-grey-text);\r\n}\r\n.content-toolbar {\r\n  height: 64px;\r\n  width: 100%;\r\n  position: relative;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.input-search {\r\n  width: 28px;\r\n  height: 28px;\r\n  position: absolute;\r\n  left: 364px;\r\n  bgs\r\n  border-left: 1px solid;\r\n  border-color: var(--button-border);\r\n\r\n}\r\n\r\n.toolbar-button {\r\n  box-sizing: border-box;\r\n  display: flex;\r\n  align-items: center;\r\n  position: absolute;\r\n  right: 16px;\r\n}\r\n\r\n.toolbar-button button {\r\n  margin-left: 8px;\r\n}\r\n\r\n.toolbar-more {\r\n  background-color: #ffffff;\r\n  width: 32px;\r\n  height: 32px;\r\n  background-size: 28px 28px;\r\n  margin-left: 8px;\r\n  border-radius: 4px;\r\n  border: 1px solid;\r\n  border-color: var(--button-border);\r\n  box-sizing: border-box;\r\n  cursor: pointer;\r\n  position: relative;\r\n}\r\n\r\n.toolbar-more:hover {\r\n  background-color: var(--background);\r\n}\r\n\r\n#btnDeleteMore {\r\n  position: absolute;\r\n  height: 40px;\r\n  top: 40px;\r\n  right: 0;\r\n  z-index: 1;\r\n}\r\n\r\nbutton#btnDeleteMore  {\r\n  padding-left: 24px;\r\n}\r\n\r\n.page-table {\r\n  position: relative;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-content: stretch;\r\n  flex: 1;\r\n  justify-content: space-between;\r\n  height: calc(100vh - 112px);\r\n  width: 100%;\r\n}\r\n\r\n.content-table {\r\n  max-width: 100%;\r\n  flex: 1;\r\n  flex-basis: 100px;\r\n  box-sizing: border-box;\r\n  height: calc(100vh - 168px);\r\n  box-sizing: border-box;\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 56px;\r\n  overflow: auto;\r\n}\r\n\r\n.toolbar-input {\r\n  margin-left: