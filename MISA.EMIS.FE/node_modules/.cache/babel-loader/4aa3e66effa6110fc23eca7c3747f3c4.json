a[i];\n          this.selectedItemList.push(item[this.propValue]);\n        }\n      } else {\n        this.isCheckAll = false;\n\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = false;\n          this.isShowTag[i] = false;\n        }\n\n        this.selectedItemList = [];\n      }\n    },\n\n    /**\n     * Khi click dấu X ở tag name thì bỏ chọn\n     * Author: VQBao - 10/8/2022\n     */\n    unselectItemTag(item, index) {\n      this.selectedItem = item[this.propValue];\n      this.isShowTag[index] = false;\n      this.isCheck[index] = false;\n      let id = this.selectedItemList.indexOf(this.selectedItem);\n      this.selectedItemList.splice(id, 1);\n      this.isCheckAll = false;\n    },\n\n    hideTagboxData() {\n      this.isShowListData = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,KAAK,EAAE;IACLC,GAAG,EAAEC,MADA;IAELC,OAAO,EAAED,MAFJ;IAGLE,SAAS,EAAEF;EAHN,CADM;;EAMbG,IAAI,GAAG;IACL,OAAO;MACLA,IAAI,EAAE,EADD;MAELC,cAAc,EAAE,KAFX;MAGLC,SAAS,EAAE,EAHN;MAILC,YAAY,EAAE,IAJT;MAKLC,gBAAgB,EAAE,EALb;MAMLC,OAAO,EAAE,EANJ;MAOLC,UAAU,EAAE;IAPP,CAAP;EASD,CAhBY;;EAiBbC,OAAO,GAAG;IACR,IAAIC,EAAC,GAAI,IAAT;IACA;;;;;IAIAd,KAAI,CACDe,GADH,CACOD,EAAE,CAACZ,GADV,EAEGc,IAFH,CAESC,GAAD,IAAS;MACbC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACX,IAAhB;MACAQ,EAAE,CAACR,IAAH,GAAUW,GAAG,CAACX,IAAd;MACA,IAAIc,EAAJ;MACAF,OAAO,CAACC,GAAR,CAAYL,EAAE,CAACR,IAAH,CAAQ,CAAR,CAAZ;IACD,CAPH,EAQGe,KARH,CAQUC,GAAD,IAAS;MACdJ,OAAO,CAACK,KAAR,CAAcD,GAAd;IACD,CAVH;EAWD,CAlCY;;EAmCbE,OAAO,EAAE;IACP;;;;IAIAC,YAAY,GAAG;MACb,KAAKlB,cAAL,GAAsB,CAAC,KAAKA,cAA5B;IACD,CAPM;;IAQP;;;;IAIAmB,cAAc,CAACC,IAAD,EAAOC,KAAP,EAAc;MAC1B,KAAKnB,YAAL,GAAoBkB,IAAI,CAAC,KAAKtB,SAAN,CAAxB;;MACA,IAAI,CAAC,KAAKM,OAAL,CAAaiB,KAAb,CAAL,EAA0B;QACxB,KAAKjB,OAAL,CAAaiB,KAAb,IAAsB,IAAtB;QACA,KAAKpB,SAAL,CAAeoB,KAAf,IAAwB,IAAxB;QACA,KAAKlB,gBAAL,CAAsBmB,IAAtB,CAA2B,KAAKpB,YAAhC;;QACA,IAAI,KAAKC,gBAAL,CAAsBoB,MAAtB,IAAgC,KAAKxB,IAAL,CAAUwB,MAA9C,EAAsD;UACp