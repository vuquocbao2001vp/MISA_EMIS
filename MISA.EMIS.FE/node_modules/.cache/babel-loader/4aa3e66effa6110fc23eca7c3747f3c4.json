le.log(res.data);\n        me.data = res.data;\n        var da\n        console.log(me.data[0]);\n      })\n      .catch((err) => {\n        console.error(err);\n      });\n  },\n  methods: {\n    /**\n     * Thực hiện ẩn hiện danh sách các select\n     * Author: VQBao - 10/8/2022\n     */\n    showListData() {\n      this.isShowListData = !this.isShowListData;\n    },\n    /**\n     * Thực hiện chọn item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectDataItem(item, index) {\n      this.selectedItem = item[this.propValue];\n      if (!this.isCheck[index]) {\n        this.isCheck[index] = true;\n        this.isShowTag[index] = true;\n        this.selectedItemList.push(this.selectedItem);\n        if (this.selectedItemList.length == this.data.length) {\n          this.isCheckAll = true;\n        }\n      } else {\n        this.isCheck[index] = false;\n        this.isShowTag[index] = false;\n        let id = this.selectedItemList.indexOf(this.selectedItem);\n        this.selectedItemList.splice(id, 1);\n        this.isCheckAll = false;\n      }\n    },\n    /**\n     * Thực hiện chọn tất cả item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectAllItem() {\n      if (!this.isCheckAll) {\n        this.isCheckAll = true;\n        this.selectedItemList = [];\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = true;\n          this.isShowTag[i] = true;\n          let item = this.data[i];\n          this.selectedItemList.push(item[this.propValue]);\n        }\n      } else {\n        this.isCheckAll = false;\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = false;\n          this.isShowTag[i] = false;\n        }\n        this.selectedItemList = [];\n      }\n    },\n    /**\n     * Khi click dấu X ở tag name thì bỏ chọn\n     * Author: VQBao - 10/8/2022\n     */\n    unselectItemTag(item, index) {\n      this.selectedItem = item[this.propValue];\n      this.isShowTag[index] = false;\n      this.isCheck[index] = false;\n      let id = this.selectedItemList.indexOf(this.selectedItem);\n      this.selectedItemList.splice(id, 1);\n      this.isCheckAll = false;\n    },\n    hideTagboxData() {\n      this.isShowListData = false;\n    }\n  },\n};\n</script>\n<style scoped>\n.is-show-tag {\n  display: flex !important;\n}\n\n.selected-item-tag {\n  position: relative;\n  min-width: fit-content;\n  height: 24px;\n  background-color: var(--button-grey-background);\n  display: none;\n  align-items: center;\n  padding-left: 8px;\n  border-radius: 4px;\n  margin-right: 8px;\n  box-sizing: border-box;\n}\n\n.selected-item-tag-icon {\n  height: 24px;\n  width: 24px;\n  cursor: pointer;\n  box-sizing: border-box;\n}\n\n.is-show {\n  display: block !important;\n}\n.tagbox {\n  position: relative;\n  width: 200px;\n  box-sizing: border-box;\n  display: flex;\n  flex-direction: column;\n}\n.tagbox-content {\n  width: 100%;\n  height: 32px;\n  border: 1px solid;\n  border-radius: 4px;\n  border-color: var(--button-border);\n  box-sizing: border-box;\n  padding: 0 4px;\n}\n\n.tagbox-content-text {\n  height: 32px;\n  width: calc(100% - 36px);\n  display: flex;\n  align-items: center;\n  box-sizing: border-box;\n  overflow-x: scroll;\n  overflow-y: hidden;\n}\n\n.tagbox-button {\n  width: 32px;\n  height: 32px;\n  position: absolute;\n  right: 0px;\n  top: 0px;\n  cursor: pointer;\n}\n\n.tagbox-button:hover {\n  background-color: var(--background);\n}\n\n.tagbox-data {\n  display: none;\n  width: 100%;\n  max-height: 200px;\n  box-sizing: border-box;\n  box-shadow: 0.5px 0.5px 1px 1px rgb(222, 222, 222);\n  border-radius: 4px;\n  padding-top: 16px;\n  padding-bottom: 12px;\n  position: absolute;\n  top: 40px;\n  overflow-y: scroll;\n  overflow-x: hidden;\n  background-color: var(--white-background);\n  z-index: 1;\n  outline: none;\n}\n\n.tagbox-item-all {\n  width: calc(100% - 32px);\n  height: 32px;\n  box-sizing: border-box;\n  cursor: pointer;\n  