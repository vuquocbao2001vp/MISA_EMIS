{"ast":null,"code":"export default {\n  props: ['isHidePopup', 'title', 'text', 'staffSelected', 'rowStaff'],\n  methods: {\n    /**\r\n     * Khi bấm vào nút đóng thì hủy thực hiện xóa\r\n     * Author: VQBao - \r\n     */\n    cancleDeleteStaff() {\n      this.$emit(\"hidePopup\", true);\n    },\n\n    confirmDeleteStaff() {\n      if (this.rowStaff.length == 1 || this.rowStaff.length == 0) {\n        this.$emit(\"removeStaff\", this.staffSelected);\n        this.$emit(\"hidePopup\", true);\n      } else {\n        this.$emit(\"removeStaffs\", this.rowStaff);\n        this.$emit(\"hidePopup\", true);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAcA,eAAe;EACbA,KAAK,EAAE,CAAC,aAAD,EAAe,OAAf,EAAwB,MAAxB,EAAgC,eAAhC,EAAiD,UAAjD,CADM;EAEbC,OAAO,EAAE;IACP;;;;IAIAC,iBAAiB,GAAG;MAClB,KAAKC,KAAL,CAAW,WAAX,EAAwB,IAAxB;IACD,CAPM;;IAQPC,kBAAkB,GAAG;MACnB,IAAG,KAAKC,QAAL,CAAcC,MAAd,IAAwB,CAAxB,IAA6B,KAAKD,QAAL,CAAcC,MAAd,IAAwB,CAAxD,EAA0D;QACxD,KAAKH,KAAL,CAAW,aAAX,EAA0B,KAAKI,aAA/B;QACA,KAAKJ,KAAL,CAAW,WAAX,EAAwB,IAAxB;MACF,CAHA,MAGO;QACL,KAAKA,KAAL,CAAW,cAAX,EAA2B,KAAKE,QAAhC;QACA,KAAKF,KAAL,CAAW,WAAX,EAAwB,IAAxB;MACF;IACF;;EAhBO;AAFI,CAAf","names":["props","methods","cancleDeleteStaff","$emit","confirmDeleteStaff","rowStaff","length","staffSelected"],"sourceRoot":"","sources":["E:\\MisaProject\\W2022_06\\MISA.EMIS.FE\\src\\components\\base\\BasePopup.vue"],"sourcesContent":["<template>\r\n  <div class=\"popup\" :class=\"{'hide-popup': isHidePopup}\">\r\n    <div class=\"popop-content\">\r\n      <div class=\"popup-close icon-center icon-X-2\" @click=\"cancleDeleteStaff\"></div>\r\n      <span class=\"popup-title\">{{title}}</span>\r\n      <span class=\"popup-text\">{{text}}</span>\r\n      <div class=\"popup-button\">\r\n        <BaseButton buttonType=\"button-white\" buttonName=\"Đóng\" @click=\"cancleDeleteStaff\"/>\r\n        <BaseButton buttonType=\"button-green\" buttonName=\"Đồng ý\" @click=\"confirmDeleteStaff\"/>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  props: ['isHidePopup','title', 'text', 'staffSelected', 'rowStaff'],\r\n  methods: {\r\n    /**\r\n     * Khi bấm vào nút đóng thì hủy thực hiện xóa\r\n     * Author: VQBao - \r\n     */\r\n    cancleDeleteStaff() {\r\n      this.$emit(\"hidePopup\", true);\r\n    },\r\n    confirmDeleteStaff() {\r\n      if(this.rowStaff.length == 1 || this.rowStaff.length == 0){\r\n        this.$emit(\"removeStaff\", this.staffSelected);\r\n        this.$emit(\"hidePopup\", true);\r\n      } else {\r\n        this.$emit(\"removeStaffs\", this.rowStaff);\r\n        this.$emit(\"hidePopup\", true);\r\n      } \r\n    }\r\n  },\r\n}\r\n</script>\r\n<style scoped>\r\n.hide-popup {\r\n  display: none !important;\r\n}\r\n.popup {\r\n  position: fixed;\r\n  align-items: center;\r\n  justify-content: center;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background-color: rgba(0, 0, 0, 0.24);\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.popop-content {\r\n  width: 400px;\r\n  background-color: #ffffff;\r\n  position: relative;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.popup-close {\r\n  width: 40px;\r\n  height: 40px;\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  cursor: pointer;\r\n}\r\n\r\n.popup-title {\r\n  font-family: Opensans-Bold;\r\n  font-size: 24px;\r\n  margin: 20px 24px 12px 24px;\r\n}\r\n\r\n.popup-text {\r\n  margin: 0 24px 16px 24px;\r\n  line-height: 24px;\r\n}\r\n\r\n.popup-button {\r\n  margin-left: calc(100% - 24px - 96px - 96px - 8px);\r\n  margin-bottom: 24px;\r\n}\r\n\r\n.popup-button button {\r\n  margin-right: 8px \r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}