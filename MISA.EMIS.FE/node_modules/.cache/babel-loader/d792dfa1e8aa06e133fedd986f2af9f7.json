{"ast":null,"code":"export default {\n  props: [\"totalResult\", \"totalPage\", \"currentPage\"],\n\n  data() {\n    return {\n      inPageNumber: null,\n      outPageNumber: null,\n      timeout: null\n    };\n  },\n\n  watch: {\n    currentPage: function (value) {\n      this.inPageNumber = value.toString();\n    },\n    outPageNumber: function (value) {\n      var pageNumber = parseInt(value);\n\n      if (pageNumber <= 0 || pageNumber == null) {\n        pageNumber = 1;\n        this.$emit(\"loadData\", 25, pageNumber, '');\n      } else if (pageNumber > this.totalPage) {\n        pageNumber = this.totalPage;\n        this.$emit(\"loadData\", 25, pageNumber, '');\n      } else {\n        this.$emit(\"loadData\", 25, pageNumber, '');\n      }\n    }\n  },\n  methods: {\n    loadPaging() {\n      clearTimeout(this.timeout);\n      var self = this;\n      this.timeout = setTimeout(function () {\n        self.outPageNumber = self.inPageNumber;\n      }, 500);\n    },\n\n    toFirstPage() {\n      if (this.currentPage != 1) {\n        this.$emit(\"loadData\", 25, 1, '');\n      }\n    },\n\n    toLastPage() {\n      if (this.currentPage != this.totalPage) {\n        this.$emit(\"loadData\", 25, this.totalPage, '');\n      }\n    },\n\n    toPrevPage() {\n      if (this.currentPage > 1) {\n        this.$emit(\"loadData\", 25, this.currentPage - 1, '');\n      }\n    },\n\n    toNextPage() {\n      if (this.currentPage < this.totalPage) {\n        this.$emit(\"loadData\", 25, this.currentPage + 1, '');\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyBA,eAAe;EACbA,KAAK,EAAE,CAAC,aAAD,EAAgB,WAAhB,EAA6B,aAA7B,CADM;;EAEbC,IAAI,GAAG;IACL,OAAO;MACHC,YAAY,EAAE,IADX;MAEHC,aAAa,EAAE,IAFZ;MAGHC,OAAO,EAAE;IAHN,CAAP;EAKD,CARY;;EASbC,KAAK,EAAE;IACLC,WAAW,EAAE,UAASC,KAAT,EAAgB;MAC3B,KAAKL,YAAL,GAAoBK,KAAK,CAACC,QAAN,EAApB;IACD,CAHI;IAILL,aAAa,EAAE,UAASI,KAAT,EAAgB;MAC7B,IAAIE,UAAS,GAAIC,QAAQ,CAACH,KAAD,CAAzB;;MACA,IAAGE,UAAS,IAAK,CAAd,IAAmBA,UAAS,IAAK,IAApC,EAA0C;QACxC