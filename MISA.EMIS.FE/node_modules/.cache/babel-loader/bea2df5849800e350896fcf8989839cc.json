        <th style=\"min-width: 60px\">Tổ chuyên môn</th>\r\n            <th style=\"min-width: 24px\">QL theo môn</th>\r\n            <th style=\"min-width: 60px\">QL kho, phòng</th>\r\n            <th style=\"min-width: 18px\">Đào tạo QLTB</th>\r\n            <th style=\"min-width: 18px\">Đang làm việc</th>\r\n            <th style=\"min-width: 24px\"></th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"staff in staffs\" :key=\"staff.employeeId\">\r\n            <td class=\"text-align-center col-checkbox\">\r\n              <BaseCheckbox />\r\n            </td>\r\n            <td class=\"text-align-center\">{{ staff.EmployeeCode }}</td>\r\n            <td\r\n              class=\"text-align-left col-full-name\"\r\n              style=\"color: var(--button-green-text)\"\r\n              @click=\"rowOnClick(staff)\"\r\n            >\r\n              {{ staff.FullName }}\r\n            </td>\r\n            <td class=\"text-align-center\">{{ staff.PhoneNumber }}</td>\r\n            <td class=\"text-align-left\">Tổ Toán-Tin</td>\r\n            <td class=\"text-align-left\">Lý</td>\r\n            <td class=\"text-align-left\">Phòng thí nghiệm</td>\r\n            <td class=\"text-align-center col-check-icon\">\r\n              <div class=\"cell-check icon-center icon-check\"></div>\r\n            </td>\r\n            <td class=\"text-align-center col-check-icon\">\r\n              <div class=\"cell-check icon-center icon-check\"></div>\r\n            </td>\r\n            <td class=\"text-align-center col-icon\">\r\n              <div\r\n                class=\"cell-edit-icon icon-center icon-edit\"\r\n                @click=\"rowOnClick(staff)\"\r\n              ></div>\r\n              <div \r\n                class=\"cell-remove-icon icon-center icon-remove\"\r\n                @click=\"removeOnClick\"\r\n              ></div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <TablePaging />\r\n  </div>\r\n  <StaffDialog\r\n    :isShow=\"isShowDialog\"\r\n    :staffSelected=\"staffSelected\"\r\n    :staffSelectedId=\"staffSelectedId\"\r\n    :btnSaveMode=\"btnSaveMode\"\r\n    @isShowStaffDialog=\"showStaffDialog\"\r\n    @loadData=\"loadData\"\r\n  />\r\n</template> \r\n<script>\r\nimport TablePaging from \"../base/TablePaging.vue\";\r\nimport BaseButton from \"../base/BaseButton.vue\";\r\nimport BaseInput from \"../base/BaseInput.vue\";\r\nimport StaffDialog from \"../dialog/StaffDialog.vue\";\r\nimport BaseCheckbox from \"../base/BaseCheckbox.vue\";\r\n\r\nimport axios from \"axios\";\r\nexport default {\r\n  components: {\r\n    TablePaging,\r\n    BaseButton,\r\n    BaseInput,\r\n    StaffDialog,\r\n    BaseCheckbox,\r\n  },\r\n  created() {\r\n    console.log(this);\r\n    this.loadData();\r\n  },\r\n  data() {\r\n    return {\r\n      isShowDialog: false,\r\n      staffs: {},\r\n      staffSelected: {},\r\n      staffSelectedId: null,\r\n      btnSaveMode: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    // thực hiện tải dữ liệu thông tin giáo viên và hiển thị trên bảng\r\n    loadData(){\r\n      try {\r\n      var me = this;\r\n      // lấy dữ liệu từ api\r\n      axios\r\n        .get(\"https://cukcuk.manhnv.net/api/v1/Employees\")\r\n        .then(function (respond) {\r\n          me.staffs = respond.data;\r\n          console.log(respond.data);\r\n        })\r\n        .catch(function (res) {\r\n          console.log(res);\r\n        });\r\n    } catch (error) {\r\n      console.log(error);\r\n    }\r\n    },\r\n    // khi bấm thêm thì form chi tiết giáo viên hiện ra\r\n    btnAddOnClick() {\r\n      // btnSaveMode = 1 => thực hiện thêm mới\r\n      this.btnSaveMode = 1;\r\n      this.showStaffDialog(true);\r\n      this.staffSelected = {};\r\n      this.staffSelectedId = null;\r\n      var me = this;\r\n      // gọi api lấy mã nhân viên mới\r\n      axios\r\n        .get(\"https://cukcuk.manhnv.net/api/v1/Employees/NewEmployeeCode\")\r\n        .then((response) => {\r\n          console.log(response);\r\n          me.staffSelected.EmployeeCode = response.data;\r\n        })\r\n        .catch((err) => {\r\n          console.error(err);\r\n        });\r\n    },\r\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của form thêm mới\r\n     * @param {Boolean} iShow - true: hiển thị, false: ẩn đi\r\n     */\r\n    showStaffDialog(isShow) {\r\n      this.isShowDialog = isShow;\r\n    },\r\n    // Khi nhấn vào tên trong bảng, hiện form thông tin chi tiết\r\n    rowOnClick(staff) {\r\n      // btnSaveMode = 0 => thực hiện sửa\r\n      this.btnSaveMode = 0;\r\n      this.staffSelected = staff;\r\n      this.staffSelectedId = staff.EmployeeId;\r\n      this.showStaffDialog(true);\r\n    },\r\n    // khi bấm vào icon xóa thì thực hiện xóa\r\n    removeOnClick(){\r\n      axios.delete('https://cukcuk.manhnv.net/api/v1/Employees')\r\n      .then(res => {\r\n        console.log(res)\r\n      })\r\n      .catch(err => {\r\n        console.error(err); \r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.content-toolbar {\r\n  height: 64px;\r\n  width: 100%;\r\n  position: relative;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.toolbar-button {\r\n  box-sizing: border-box;\r\n  display: flex;\r\n  align-items: center;\r\n  position: absolute;\r\n  right: 16px;\r\n}\r\n\r\n.toolbar-button button {\r\n  margin-left: 8px;\r\n}\r\n\r\n.toolbar-more {\r\n  background-color: #ffffff;\r\n  width: 32px;\r\n  height: 32px;\r\n  background-size: 28px 28px;\r\n  margin-left: 8px;\r\n  border-radius: 4px;\r\n  border: 1px solid;\r\n  border-color: var(--button-border);\r\n  box-sizing: border-box;\r\n  cursor: pointer;\r\n}\r\n\r\n.page-table {\r\n  position: relative;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-content: stretch;\r\n  flex: 1;\r\n  justify-content: space-between;\r\n  height: calc(100vh - 112px);\r\n  width: 100%;\r\n}\r\n\r\n.content-table {\r\n  max-width: 100%;\r\n  flex: 1;\r\n  flex-basis: 100px;\r\n  box-sizing: border-box;\r\n  height: calc(100vh - 168px);\r\n  box-sizing: border-box;\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 56px;\r\n  overflow: auto;\r\n}\r\n\r\n.toolbar-input {\r\n  margin-left: 24px;\r\n  width: 376px;\r\n  background-size: 28px 28px;\r\n}\r\n\r\n.col-full-name,\r\n.col-checkbox,\r\n.col-icon,\r\n.col-check-icon {\r\n  cursor: pointer;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}