{"ast":null,"code":"import TablePaging from \"../base/TablePaging.vue\";\nimport BaseButton from \"../base/BaseButton.vue\";\nimport BaseInput from \"../base/BaseInput.vue\";\nimport StaffDialog from \"../dialog/StaffDialog.vue\";\nimport BaseCheckbox from \"../base/BaseCheckbox.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    TablePaging,\n    BaseButton,\n    BaseInput,\n    StaffDialog,\n    BaseCheckbox\n  },\n\n  created() {\n    console.log(this);\n    this.loadData();\n  },\n\n  data() {\n    return {\n      isShowDialog: false,\n      staffs: {},\n      staffSelected: {},\n      staffSelectedId: null,\n      btnSaveMode: 0\n    };\n  },\n\n  methods: {\n    // thực hiện tải dữ liệu thông tin giáo viên và hiển thị trên bảng\n    loadData() {\n      try {\n        var me = this; // lấy dữ liệu từ api\n\n        axios.get(\"https://cukcuk.manhnv.net/api/v1/Employees\").then(function (respond) {\n          me.staffs = respond.data;\n          console.log(respond.data);\n        }).catch(function (res) {\n          console.log(res);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    // khi bấm thêm thì form chi tiết giáo viên hiện ra\n    btnAddOnClick() {\n      // btnSaveMode = 1 => thực hiện thêm mới\n      this.btnSaveMode = 1;\n      this.showStaffDialog(true);\n      this.staffSelected = {};\n      this.staffSelectedId = null;\n      var me = this; // gọi api lấy mã nhân viên mới\n\n      axios.get(\"https://cukcuk.manhnv.net/api/v1/Employees/NewEmployeeCode\").then(response => {\n        console.log(response);\n        me.staffSelected.EmployeeCode = response.data;\n      }).catch(err => {\n        console.error(err);\n      });\n    },\n\n    /**\r\n     * Thay đổi trạng thái ẩn hiện của form thêm mới\r\n     * @param {Boolean} iShow - true: hiển thị, false: ẩn đi\r\n     */\n    showStaffDialog(isShow) {\n      this.isShowDialog = isShow;\n    },\n\n    // Khi nhấn vào tên trong bảng, hiện form thông tin chi tiết\n    rowOnClick(staff) {\n      // btnSaveMode = 0 => thực hiện sửa\n      this.btnSaveMode = 0;\n      this.staffSelected = staff;\n      this.staffSelectedId = staff.EmployeeId;\n      this.showStaffDialog(true);\n    },\n\n    // khi bấm vào icon xóa thì thực hiện xóa\n    removeOnClick() {\n      axios.delete('https://cukcuk.manhnv.net/api/v1/Employees').then(res => {\n        console.log(res);\n      }).catch(err => {\n        console.error(err);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0FA,OAAOA,WAAP,MAAwB,yBAAxB;AACA,OAAOC,UAAP,MAAuB,wBAAvB;AACA,OAAOC,SAAP,MAAsB,uBAAtB;AACA,OAAOC,WAAP,MAAwB,2BAAxB;AACA,OAAOC,YAAP,MAAyB,0BAAzB;AAEA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,UAAU,EAAE;IACVN,WADU;IAEVC,UAFU;IAGVC,SAHU;IAIVC,WAJU;IAKVC;EALU,CADC;;EAQbG,OAAO,GAAG;IACRC,OAAO,CAACC,GAAR,CAAY,IAAZ;IACA,KAAKC,QAAL;EACD,CAXY;;EAYbC,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,KADT;MAELC,MAAM,EAAE,EAFH;MAGLC,aAAa,EAAE,EAHV;MAILC,eAAe,EAAE,IAJZ;MAKLC,WAAW,EAAE;IALR,CAAP;EAOD,CApBY;;EAqBbC,OAAO,EAAE;IACP;IACAP,QAAQ,GAAE;MACR,IAAI;QACJ,IAAIQ,EAAC,GAAI,IAAT,CADI,CAEJ;;QACAb,KAAI,CACDc,GADH,CACO,4CADP,EAEGC,IAFH,CAEQ,UAAUC,OAAV,EAAmB;UACvBH,EAAE,CAACL,MAAH,GAAYQ,OAAO,CAACV,IAApB;UACAH,OAAO,CAACC,GAAR,CAAYY,OAAO,CAACV,IAApB;QACD,CALH,EAMGW,KANH,CAMS,UAAUC,GAAV,EAAe;UACpBf,OAAO,CAACC,GAAR,CAAYc,GAAZ;QACD,CARH;MASF,CAZE,CAYA,OAAOC,KAAP,EAAc;QACdhB,OAAO,CAACC,GAAR,CAAYe,KAAZ;MACF;IACC,CAlBM;;IAmBP;IACAC,aAAa,GAAG;MACd;MACA,KAAKT,WAAL,GAAmB,CAAnB;MACA,KAAKU,eAAL,CAAqB,IAArB;MACA,KAAKZ,aAAL,GAAqB,EAArB;MACA,KAAKC,eAAL,GAAuB,IAAvB;MACA,IAAIG,EAAC,GAAI,IAAT,CANc,CAOd;;MACAb,KAAI,CACDc,GADH,CACO,4DADP,EAEGC,IAFH,CAESO,QAAD,IAAc;QAClBnB,OAAO,CAACC,GAAR,CAAYkB,QAAZ;QACAT,EAAE,CAACJ,aAAH,CAAiBc,YAAjB,GAAgCD,QAAQ,CAAChB,IAAzC;MACD,CALH,EAMGW,KANH,CAMUO,GAAD,IAAS;QACdrB,OAAO,CAACgB,KAAR,CAAcK,GAAd;MACD,CARH;IASD,CArCM;;IAsCP;;;;IAIAH,eAAe,CAACI,MAAD,EAAS;MACtB,KAAKlB,YAAL,GAAoBkB,MAApB;IACD,CA5CM;;IA6CP;IACAC,UAAU,CAACC,KAAD,EAAQ;MAChB;MACA,KAAKhB,WAAL,