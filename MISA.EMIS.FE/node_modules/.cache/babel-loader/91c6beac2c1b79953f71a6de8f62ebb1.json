{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  props: {\n    url: String,\n    propKey: String,\n    propValue: String\n  },\n\n  data() {\n    return {\n      data: [],\n      isShowListData: false,\n      selectedItem: null,\n      selectedItemList: [],\n      isCheck: [],\n      isCheckAll: false\n    };\n  },\n\n  created() {\n    var me = this;\n    /**\n     * Thực hiện lấy dữ liệu từ api\n     * Author: VQBao - 10/8/2022\n     */\n\n    axios.get(me.url).then(res => {\n      me.data = res.data;\n      console.log(res.data[0][me.propValue]);\n      me.selectedItem = res.data[1][me.propValue];\n    }).catch(err => {\n      console.error(err);\n    });\n  },\n\n  methods: {\n    /**\n     * Thực hiện ẩn hiện danh sách các select\n     * Author: VQBao - 10/8/2022\n     */\n    showListData() {\n      this.isShowListData = !this.isShowListData;\n    },\n\n    /**\n     * Thực hiện chọn item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectDataItem(item, index) {\n      this.selectedItem = item[this.propValue];\n\n      if (!this.isCheck[index]) {\n        this.isCheck[index] == true;\n        this.selectedItemL.push(item[this.propValue]);\n        this.isShowListData = false;\n        console.log(this.selectedItem);\n      }\n    },\n\n    selectAllItem() {\n      this.isCheckAll = !this.isCheckAll;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,KAAK,EAAE;IACLC,GAAG,EAAEC,MADA;IAELC,OAAO,EAAED,MAFJ;IAGLE,SAAS,EAAEF;EAHN,CADM;;EAMbG,IAAI,GAAG;IACL,OAAO;MACLA,IAAI,EAAE,EADD;MAELC,cAAc,EAAE,KAFX;MAGLC,YAAY,EAAE,IAHT;MAILC,gBAAgB,EAAE,EAJb;MAKLC,OAAO,EAAE,EALJ;MAMLC,UAAU,EAAE;IANP,CAAP;EAQD,CAfY;;EAgBbC,OAAO,GAAG;IACR,IAAIC,EAAC,GAAI,IAAT;IACA;;;;;IAIAb,KAAI,CACDc,GADH,CACOD,EAAE,CAACX,GADV,EAEGa,IAFH,CAESC,GAAD,IAAS;MACbH,EAAE,CAACP,IAAH,GAAUU,GAAG,CAACV,IAAd;MACAW,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACV,IAAJ,CAAS,CAAT,EAAYO,EAAE,CAACR,SAAf,CAAZ;MACAQ,EAAE,CAACL,YAAH,GAAkBQ,GAAG,CAACV,IAAJ,CAAS,CAAT,EAAYO,EAAE,CAACR,SAAf,CAAlB;IACD,CANH,EAOGc,KAPH,CAOUC,GAAD,IAAS;MACdH,OAAO,CAACI,KAAR,CAAcD,GAAd;IACD,CATH;EAUD,CAhCY;;EAiCbE,OAAO,EAAE;IACP;;;;IAIAC,YAAY,GAAG;MACb,KAAKhB,cAAL,GAAsB,CAAC,KAAKA,cAA5B;IACD,CAPM;;IAQP;;;;IAIAiB,cAAc,CAACC,IAAD,EAAOC,KAAP,EAAc;MAC1B,KAAKlB,YAAL,GAAoBiB,IAAI,CAAC,KAAKpB,SAAN,CAAxB;;MACA,IAAG,CAAC,KAAKK,OAAL,CAAagB,KAAb,CAAJ,EAAyB;QACvB,KAAKhB,OAAL,CAAagB,KAAb,KAAuB,IAAvB;QACA,KAAKC,aAAL,CAAmBC,IAAnB,CAAwBH,IAAI,CAAC,KAAKpB,SAAN,CAA5B;QACA,KAAKE,cAAL,GAAsB,KAAtB;QACAU,OAAO,CAACC,GAAR,CAAY,KAAKV,YAAjB;MACF;IACD,CApBM;;IAqBPqB,aAAa,GAAG;MACd,KAAKlB,UAAL,GAAkB,CAAC,KAAKA,UAAxB;IACF;;EAvBO;AAjCI,CAAf","names":["axios","props","url","String","propKey","propValue","data","isShowListData","selectedItem","selectedItemList","isCheck","isCheckAll","created","me","get","then","res","console","log","catch","err","error","methods","showListData","selectDataItem","item","index","selectedItemL","push","selectAllItem"],"sourceRoot":"","sources":["E:\\MisaProject\\W2022_06\\misa_emis\\src\\components\\base\\BaseTagboxCustom.vue"],"sourcesContent":["<template>\n  <div class=\"tagbox\" @keyup.enter=\"showListData\">\n    <div class=\"tagbox-content\">{{this.selectedItem}}</div>\n    <div\n      class=\"tagbox-button icon-center icon-Chevron\"\n      @click=\"showListData\"\n    ></div>\n    <div class=\"tagbox-data\" :class=\"{ 'is-show': isShowListData }\">\n      <div class=\"tagbox-item\">\n        <BaseCheckbox :class=\"isCheckAll ? 'checkbox-active' : 'checkbox-inactive'\" @click=\"selectAllItem\"/>\n        <div class=\"tagbox-item-content\">Tất cả</div>\n      </div>\n      <div class=\"tagbox-item\" v-for=\"(item, index) in data\" :key=\"item[this.propKey]\" @click=\"selectDataItem(item, index)\">\n        <BaseCheckbox :class=\"isCheck[index] ? 'checkbox-active' : 'checkbox-inactive'\" @click=\"selectDataItem(item, index)\"/>\n        <div class=\"tagbox-item-content\">{{item[this.propValue]}}</div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  props: {\n    url: String,\n    propKey: String,\n    propValue: String,\n  },\n  data() {\n    return {\n      data: [],\n      isShowListData: false,\n      selectedItem: null,\n      selectedItemList: [],\n      isCheck: [],\n      isCheckAll: false,\n    };\n  },\n  created() {\n    var me = this;\n    /**\n     * Thực hiện lấy dữ liệu từ api\n     * Author: VQBao - 10/8/2022\n     */\n    axios\n      .get(me.url)\n      .then((res) => {\n        me.data = res.data;\n        console.log(res.data[0][me.propValue]);\n        me.selectedItem = res.data[1][me.propValue];\n      })\n      .catch((err) => {\n        console.error(err);\n      });\n  },\n  methods: {\n    /**\n     * Thực hiện ẩn hiện danh sách các select\n     * Author: VQBao - 10/8/2022\n     */\n    showListData() {\n      this.isShowListData = !this.isShowListData;\n    },\n    /**\n     * Thực hiện chọn item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectDataItem(item, index) {\n      this.selectedItem = item[this.propValue];\n      if(!this.isCheck[index]) {\n        this.isCheck[index] == true;\n        this.selectedItemL.push(item[this.propValue])\n        this.isShowListData = false;\n        console.log(this.selectedItem);\n      }\n    },\n    selectAllItem() {\n      this.isCheckAll = !this.isCheckAll;\n    }\n  },\n};\n</script>\n<style scoped>\n.is-show {\n  display: block !important;\n}\n.tagbox {\n  position: relative;\n  width: 200px;\n  box-sizing: border-box;\n  display: flex;\n  flex-direction: column;\n}\n.tagbox-content {\n  width: 200px;\n  height: 32px;\n  border: 1px solid;\n  border-radius: 4px;\n  border-color: var(--button-border);\n  box-sizing: border-box;\n  display: flex;\n  align-items: center;\n  padding: 0 16px;\n}\n\n.tagbox-button {\n  width: 32px;\n  height: 32px;\n  position: absolute;\n  right: 0px;\n  top: 0px;\n  cursor: pointer;\n}\n\n.tagbox-button:hover {\n  background-color: var(--background);\n}\n\n.tagbox-data {\n  display: none;\n  width: 200px;\n  max-height: 200px;\n  box-sizing: border-box;\n  box-shadow: 0.5px 0.5px 1px 1px rgb(222, 222, 222);\n  border-radius: 4px;\n  padding-top: 4px;\n  padding-bottom: 4px;\n  position: absolute;\n  top: 36px;\n  overflow-y: scroll;\n  overflow-x: hidden;\n  background-color: var(--white-background);\n  z-index: 1;\n  outline: none;\n}\n\n.tagbox-item {\n  width: 200px;\n  height: 32px;\n  box-sizing: border-box;\n  cursor: pointer;\n  padding: 0 16px;\n  display: flex;\n  align-items: center;\n}\n\n.tagbox-item-content {\n  margin-left: 12px;\n}\n\n.tagbox-item:hover {\n  background-color: var(--grid-line-focus-hover);\n}\n\n/* custom scroll*/ \n/* width */\n.tagbox-data::-webkit-scrollbar {\n  width: 4px;\n  height: 4px;\n}\n\n/* Track */\n.tagbox-data::-webkit-scrollbar-track {\n  border-radius: 4px;\n}\n\n/* Handle */\n.tagbox-data::-webkit-scrollbar-thumb {\n  background: var(--scroll-color);\n  border-radius: 4px;\n}\n/* hover */\n.tagbox-data::-webkit-scrollbar-thumb:hover {\n  background: var(--scroll-hover-color);\n}\n</style>"]},"metadata":{},"sourceType":"module"}