{"ast":null,"code":"export default {\n  props: [\"totalResult\", \"totalPage\", \"currentPage\"],\n\n  data() {\n    return {\n      inPageNumber: null,\n      outPageNumber: null,\n      timeout: null\n    };\n  },\n\n  watch: {\n    currentPage: function (value) {\n      this.pageNumber = value.toString();\n    },\n    pageNumber: function (value) {\n      var number = parseInt(value);\n      this.$emit(\"loadData\", 25, number, '');\n    }\n  },\n  methods: {\n    loadPaging() {\n      clearTimeout(this.timeout);\n      var self = this;\n      this.timeout = setTimeout(function () {\n        self.outputTextSearch = self.inputTextSearch;\n      }, 500);\n    },\n\n    toFirstPage() {\n      if (this.currentPage != 1) {\n        this.$emit(\"loadData\", 25, 1, '');\n      }\n    },\n\n    toLastPage() {\n      if (this.currentPage != this.totalPage) {\n        this.$emit(\"loadData\", 25, this.totalPage, '');\n      }\n    },\n\n    toPrevPage() {\n      if (this.currentPage > 1) {\n        this.$emit(\"loadData\", 25, this.currentPage - 1, '');\n      }\n    },\n\n    toNextPage() {\n      if (this.currentPage < this.totalPage) {\n        this.$emit(\"loadData\", 25, this.currentPage + 1, '');\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyBA,eAAe;EACbA,KAAK,EAAE,CAAC,aAAD,EAAgB,WAAhB,EAA6B,aAA7B,CADM;;EAEbC,IAAI,GAAG;IACL,OAAO;MACHC,YAAY,EAAE,IADX;MAEHC,aAAa,EAAE,IAFZ;MAGHC,OAAO,EAAE;IAHN,CAAP;EAKD,CARY;;EASbC,KAAK,EAAE;IACLC,WAAW,EAAE,UAASC,KAAT,EAAgB;MAC3B,KAAKC,UAAL,GAAkBD,KAAK,CAACE,QAAN,EAAlB;IACD,CAHI;IAILD,UAAU,EAAE,UAASD,KAAT,EAAgB;MAC1B,IAAIG,MAAK,GAAIC,QAAQ,CAACJ,KAAD,CAArB;MACA,KAAKK,KAAL,CAAW,UAAX,EAAuB,EAAvB,EAA2BF,MAA3B,EAAkC,EAAlC;IACF;EAPK,CATM;EAkBbG,OAAO,EAAE;IACPC,UAAU,GAAG;MACXC,YAAY,CAAC,KAAKX,OAAN,CAAZ;MACA,IAAIY,IAAG,GAAI,IAAX;MACA,KAAKZ,OAAL,GAAea,UAAU,CAAC,YAAY;QACpCD,IAAI,CAACE,gBAAL,GAAwBF,IAAI,CAACG,eAA7B;MACD,CAFwB,EAEtB,GAFsB,CAAzB;IAGD,CAPM;;IAQPC,WAAW,GAAG;MACZ,IAAI,KAAKd,WAAL,IAAoB,CAAxB,EAA2B;QACzB,KAAKM,KAAL,CAAW,