s.isShowListData = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.is-show {\r\n  display: block !important;\r\n}\r\n.combobox {\r\n  position: relative;\r\n  width: 200px;\r\n  box-sizing: border-box;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n.combobox-content {\r\n  width: 200px;\r\n  height: 32px;\r\n  border: 1px solid;\r\n  border-radius: 4px;\r\n  border-color: var(--button-border);\r\n  box-sizing: border-box;\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 0 16px;\r\n}\r\n\r\n.combobox-button {\r\n  width: 32px;\r\n  height: 32px;\r\n  position: absolute;\r\n  right: 0px;\r\n  top: 0px;\r\n  cursor: pointer;\r\n}\r\n\r\n.combobox-button:hover {\r\n  background-color: var(--background);\r\n}\r\n\r\n.combobox-data {\r\n  display: none;\r\n  width: 200px;\r\n  max-height: 200px;\r\n  box-sizing: border-box;\r\n  box-shadow: 0.5px 0.5px 1px 1px rgb(222, 222, 222);\r\n  border-radius: 4px;\r\n  padding-top: 4px;\r\n  padding-bottom: 4px;\r\n  position: absolute;\r\n  top: 40px;\r\n  overflow-y: scroll;\r\n  overflow-x: hidden;\r\n  background-color: var(--white-background);\r\n  z-index: 1;\r\n  outline: none;\r\n}\r\n\r\n.combobox-item {\r\n  width: 200px;\r\n  height: 32px;\r\n  /* box-sizing: border-box; */\r\n  cursor: pointer;\r\n  padding: 0 16px;\r\n}\r\n\r\n.combobox-item:hover {\r\n  background-color: var(--grid-line-focus-hover);\r\n}\r\n\r\n/* custom scroll*/ \r\n/* width */\r\n.combobox-data::-webkit-scrollbar {\r\n  width: 4px;\r\n  height: 4px;\r\n  position: absolute;\r\n  \r\n}\r\n\r\n/* Track */\r\n.combobox-data::-webkit-scrollbar-track {\r\n  border-radius: 4px;\r\n}\r\n\r\n/* Handle */\r\n.combobox-data::-webkit-scrollbar-thumb {\r\n  background: var(--scroll-color);\r\n  border-radius: 4px;\r\n}\r\n/* hover */\r\n.combobox-data::-webkit-scrollbar-thumb:hover {\r\n  background: var(--scroll-hover-color);\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}