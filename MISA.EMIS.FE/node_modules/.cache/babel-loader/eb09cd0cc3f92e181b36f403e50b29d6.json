{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  props: {\n    url: String,\n    propKey: String,\n    propValue: String\n  },\n\n  data() {\n    return {\n      data: [],\n      isShowListData: false,\n      selectedItem: null,\n      selectedItemList: [],\n      isCheck: [false],\n      isCheckAll: false\n    };\n  },\n\n  created() {\n    var me = this;\n    /**\n     * Thực hiện lấy dữ liệu từ api\n     * Author: VQBao - 10/8/2022\n     */\n\n    axios.get(me.url).then(res => {\n      me.data = res.data;\n      me.selectedItem = res.data[1][me.propValue];\n    }).catch(err => {\n      console.error(err);\n    });\n  },\n\n  methods: {\n    /**\n     * Thực hiện ẩn hiện danh sách các select\n     * Author: VQBao - 10/8/2022\n     */\n    showListData() {\n      this.isShowListData = !this.isShowListData;\n    },\n\n    /**\n     * Thực hiện chọn item trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectDataItem(item, index) {\n      this.selectedItem = item[this.propValue];\n\n      if (!this.isCheck[index]) {\n        this.isCheck[index] = true;\n        this.selectedItemList.push(this.selectedItem);\n\n        if (this.selectedItemList.length == this.data.length) {\n          this.isCheckAll = true;\n        }\n\n        console.log(\"abc\" + this.selectedItemList);\n      } else {\n        this.isCheck[index] = false;\n        let id = this.selectedItemList.indexOf(this.selectedItem);\n        this.selectedItemList.splice(id, 1);\n        this.isCheckAll = false;\n        console.log(\"bcd\" + this.selectedItemList);\n      }\n    },\n\n    /**\n     * Thực hiện chọn tất cả option trong danh sách\n     * Author: VQBao - 10/8/2022\n     */\n    selectAllItem() {\n      if (!this.isCheckAll) {\n        this.isCheckAll = true;\n        this.selectedItemList = [];\n\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = true;\n          let item = this.data[i];\n          this.selectedItemList.push(item[this.propValue]);\n        }\n\n        console.log(this.selectedItemList);\n      } else {\n        this.isCheckAll = false;\n\n        for (let i = 0; i < this.data.length; i++) {\n          this.isCheck[i] = false;\n        }\n\n        this.selectedItemList = [];\n        console.log(this.selectedItemList);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,KAAK,EAAE;IACLC,GAAG,EAAEC,MADA;IAELC,OAAO,EAAED,MAFJ;IAGLE,SAAS,EAAEF;EAHN,CADM;;EAMbG,IAAI,GAAG;IACL,OAAO;MACLA,IAAI,EAAE,EADD;MAELC,cAAc,EAAE,KAFX;MAGLC,YAAY,EAAE,IAHT;MAILC,gBAAgB,EAAE,EAJb;MAKLC,OAAO,EAAE,CAAC,KAAD,CALJ;MAMLC,UAAU,EAAE;IANP,CAAP;EAQD,CAfY;;EAgBbC,OAAO,GAAG;IACR,IAAIC,EAAC,GAAI,IAAT;IACA;;;;;IAIAb,KAAI,CACDc,GADH,CACOD,EAAE,CAACX,GADV,EAEGa,IAFH,CAESC,GAAD,IAAS;MACbH,EAAE,CAACP,IAAH,GAAUU,GAAG,CAACV,IAAd;MACAO,EAAE,CAACL,YAAH,GAAkBQ,GAAG,CAACV,IAAJ,CAAS,CAAT,EAAYO,EAAE,CAACR,SAAf,CAAlB;IACD,CALH,EAMGY,KANH,CAMUC,GAAD,IAAS;MACdC,OAAO,CAACC,KAAR,CAAcF,GAAd;IACD,CARH;EASD,CA/BY;;EAgCbG,OAAO,EAAE;IACP;;;;IAIAC,YAAY,GAAG;MACb,KAAKf,cAAL,GAAsB,CAAC,KAAKA,cAA5B;IACD,CAPM;;IAQP;;;;IAIAgB,cAAc,CAACC,IAAD,EAAOC,KAAP,EAAc;MAC1B,KAAKjB,YAAL,GAAoBgB,IAAI,CAAC,KAAKnB,SAAN,CAAxB;;MACA,IAAG,CAAC,KAAKK,OAAL,CAAae,KAAb,CAAJ,EAAyB;QACvB,KAAKf,OAAL,CAAae,KAAb,IAAsB,IAAtB;QACA,KAAKhB,gBAAL,CAAsBiB,IAAtB,CAA2B,KAAKlB,YAAhC;;QACA,IAAG,KAAKC,gBAAL,CAAsBkB,MAAtB,IAAgC,KAAKrB,IAAL,CAAUqB,MAA7C,EAAoD;UAClD,KAAKhB,UAAL,GAAkB,IAAlB;QACF;;QACAQ,OAAO,CAACS,GAAR,CAAY,QAAQ,KAAKnB,gBAAzB;MACF,CAPA,MAOO;QACL,KAAKC,OAAL,CAAae,KAAb,IAAsB,KAAtB;QACA,IAAII,EAAC,GAAI,KAAKpB,gBAAL,CAAsBqB,OAAtB,CAA8B,KAAKtB,YAAnC,CAAT;QACA,KAAKC,gBAAL,CAAsBsB,MAAtB,CAA6BF,EAA7B,EAAiC,CAAjC;QACA,KAAKlB,UAAL,GAAkB,KAAlB;QACAQ,OAAO,CAACS,GAAR,CAAY,QAAQ,KAAKnB,gBAAzB;MACF;IACD,CA5BM;;IA6BP;;;;IAIAuB,aAAa,GAAG;MACd,IAAG,CAAC,KAAKrB,UAAT,EAAqB;QACnB,KAAKA,UAAL,GAAkB,IAAlB;QACA,KAAKF,gBAAL,GAAwB,EAAxB;;QACA,KAAK,IAAIwB,IAAI,CAAb,EAAgBA,IAAI,KAAK3B,IAAL,CAAUqB,MAA9B,EAAsCM,CAAC,EAAvC,EAA2C;UACzC,