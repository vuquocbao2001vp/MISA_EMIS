\r\n      }\r\n    },\r\n    /**\r\n     * Khi click vào nút về trang đầu\r\n     * Author: VQBao - 26/7/2022\r\n     */\r\n    toFirstPage() {\r\n      if (this.currentPage != 1) {\r\n        this.$emit(\"loadData\", 25, 1, this.filterName);\r\n      }\r\n    },\r\n    /**\r\n     * Khi click vào nút về trang cuối\r\n     * Author: VQBao - 26/7/2022\r\n     */\r\n    toLastPage() {\r\n      if (this.currentPage != this.totalPage) {\r\n        this.$emit(\"loadData\", 25, this.totalPage, this.filterName);\r\n      }\r\n    },\r\n    /**\r\n     * Khi click vào nút chuyển trang tiếp theo\r\n     * Author: VQBao - 26/7/2022\r\n     */\r\n    toPrevPage() {\r\n      if (this.currentPage > 1) {\r\n        this.$emit(\"loadData\", 25, this.currentPage - 1, this.filterName);\r\n      }\r\n    },\r\n    \r\n    toNextPage() {\r\n      if (this.currentPage < this.totalPage) {\r\n        this.$emit(\"loadData\", 25, this.currentPage + 1, this.filterName);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\ninput {\r\n  min-width: 64px;\r\n  padding: 0 8px;\r\n  text-align: center;\r\n  outline: none;\r\n  border: 1px solid;\r\n  border-color: var(--button-border);\r\n  border-radius: 4px;\r\n  box-sizing: border-box;\r\n}\r\n\r\ninput:focus {\r\n  border-color: var(--input-focus-border-color);\r\n}\r\n\r\n.paging {\r\n  height: 56px;\r\n  width: 100%;\r\n  position: absolute;\r\n  bottom: 0;\r\n  background-color: var(--white-background);\r\n  display: flex;\r\n  align-items: center;\r\n  padding-left: 16px;\r\n  box-sizing: border-box;\r\n}\r\n.paging-nav.icon-center {\r\n  width: 24px;\r\n  height: 24px;\r\n  background-size: cover;\r\n  margin-right: 20px;\r\n  cursor: pointer;\r\n}\r\n.paging-number {\r\n  width: 64px;\r\n  height: 32px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  border: 1px solid;\r\n  border-radius: 4px;\r\n  border-color: var(--button-border);\r\n  cursor: text;\r\n  margin-right: 20px;\r\n}\r\n</